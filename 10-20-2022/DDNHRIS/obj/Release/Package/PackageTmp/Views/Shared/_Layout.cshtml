    <!-- /main navbar -->
    <!-- Page container -->
    <div class="page-container">
        <!-- Page content -->
        <div class="page-content">
            <!-- Main sidebar -->
            <div class="sidebar sidebar-main sidebar-fixed">
                <div class="sidebar-content">
                    <!-- User menu -->
                    <div class="sidebar-user">
                        <div class="category-content">
                            <div class="media">

                                @*@{
                                        var imageBase64 = HttpContext.Current.Session["UImage"];
                                        <a href="#" class="media-left"><img src="@imageBase64" class="img-circle img-sm" alt=""></a>
                                    }*@


                                <div class="media-body">
                                    @*    <span class="media-heading text-semibold">@Session["_FullName"].ToString()</span>*@
                                    <div class="text-muted">
                                        <span style="font-family:'Arial Narrow', Arial, sans-serif">
                                            @*@Session["_PositionTitle"].ToString()*@
                                        </span>
                                    </div>
                                </div>
                                <div class="media-right media-middle">
                                    <ul class="icons-list">
                                        <li>
                                            <a href="#"><i class="icon-cog3"></i></a>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- /user menu -->
                    <!-- Main navigation -->
                    <div class="sidebar-category sidebar-category-visible">
                        <div class="category-content no-padding">
                            <ul class="navigation navigation-main navigation-accordion">


                                @{

                                    List<DDNHRIS.Models.MainMenuViewModel> myList = (List<DDNHRIS.Models.MainMenuViewModel>)Session["MenuList"];

                                    if (myList != null)
                                    {
                                        foreach (var main in myList)
                                        {
                                            <li class="navigation-header" style="margin-bottom: 0px; line-height: 1; padding-bottom: 0px !important"><span class="text-semibold" style="font-size:14px!important; ">@main.mainMenuName</span> <i class="icon-menu"></i></li>
                                            foreach (var item in main.menuList)
                                            {
                                                <li id="@item.groupCode">
                                                    @if (item.levelNo == 0)
                                                    {
                                                        <a href="~/@item.controllerName/@item.methodName">
                                                            <i class="@item.groupIcon"></i>&nbsp;<span></span>
                                                            @item.menuName
                                                        </a>
                                                    }
                                                    else
                                                    {
                                                        <a href="#">
                                                            <i class="@item.groupIcon"></i>&nbsp;<span>@item.groupName</span>
                                                        </a>
                                                    }

                                                    @if (item.levelNo == 1)
                                                    {
                                                        <ul>
                                                            @foreach (var i in item.menuLink)
                                                            {
                                                                <li id="@i.menuCode" class="mySelMenu"><a href="~/@i.controllerName/@i.methodName">@i.menuName</a></li>
                                                            }
                                                        </ul>
                                                    }
                                                    else if (item.levelNo == 2)
                                                    {

                                                        <ul>
                                                            @foreach (var itm in item.subGroupList)
                                                            {

                                                                <li class="mySelMenu">
                                                                    <a href="#"><i class="@itm.subFontIcon"></i> @itm.subGroupName</a>
                                                                    <ul>
                                                                        @foreach (var i in itm.menuLink)
                                                                        {
                                                                            <li id="@i.menuCode" class=""><a href="~/@i.controllerName/@i.methodName">&nbsp;&nbsp; @i.menuName</a></li>
                                                                        }
                                                                    </ul>
                                                                </li>
                                                            }
                                                        </ul>
                                                    }
                                                </li>
                                            }

                                        }

                                    }
                                    else
                                    {
                                        <li class="navigation-header" style="margin-bottom: 0px; line-height: 1; padding-bottom: 0px !important"><span class="text-semibold" style="font-size:14px!important; ">SPMS</span> <i class="icon-menu"></i></li>
                                        @*                                        <li class=" list-group-item"><a href="http://localhost:2025/MFO/Index">MFO Standard</a></li>
                                        *@

                                        <li class="">
                                            <a href="#" class="has-ul"><i class="icon-stack2"></i> <span>MAJOR FINAL OUTPUT</span></a>
                                            <ul class="hidden-ul" style="display: none;">
                                                <li>
                                                    <a>MFO (Unique)</a>
                                                    <ul>
                                                        <li><a href="../MFOStandard/Office"><i class="icon-list"></i>List of MFO</a></li>
                                                        <li><a href="../MFOStandard/RequestChanges"><i class="icon-clippy"></i>Calibration</a></li>
                                                    </ul>
                                                </li>
                                                <li><a href="../MFOStandard/CommonMFO">MFO (Common)</a></li>

                                            </ul>
                                        </li>

                                        <li class="">
                                            <a href="#" class="has-ul"><i class="icon-stack2"></i> <span>OPCR</span></a>
                                            <ul class="hidden-ul" style="display: none;">
                                                <li><a href="../OPCR/Target">Target</a></li>
                                                <li><a href="../OPCR/Actual">Actual</a></li>
                                            </ul>
                                        </li>
                                        <li class="">
                                            <a href="#" class="has-ul"><i class="icon-stack2"></i> <span>DPCR</span></a>
                                            <ul class="hidden-ul" style="display: none;">
                                                <li><a href="../SPMS_DPCR/DPCR">Target</a></li>
                                                <li><a href="../SPMS_DPCR/DPCR_Actual">Actual</a></li>
                                            </ul>
                                        </li>
                                        <li class="">
                                            <a href="#" class="has-ul"><i class="icon-stack2"></i> <span>IPCR</span></a>
                                            <ul class="hidden-ul" style="display: none;">
                                                <li><a href="../SPMS_IPCR/SPMS_IPCR">TARGET</a></li>
                                                <li><a href="../SPMS_IPCR/SPMS_IPCR_MPOR">MPOR</a></li>
                                                <li><a href="../SPMS_IPCR/SPMS_IPCR_SUBMITTED">SUBMITTED IPCRs</a></li>
                                            </ul>
                                        </li>


                                    }

                                }





                                <!-- /page kits -->

                            </ul>
                        </div>
                    </div>
                    <!-- /main navigation -->
                </div>
            </div>
            <!-- /main sidebar -->
            <!-- Main content -->

            <div class="content-wrapper" style="overflow-y: hidden; ">
                @RenderBody()
            </div>



        </div>
