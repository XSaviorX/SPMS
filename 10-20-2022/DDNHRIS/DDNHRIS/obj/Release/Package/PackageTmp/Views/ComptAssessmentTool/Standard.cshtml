@{
    ViewBag.Title = "Standard";
}


<link href="~/Content/assets/plugins/select2/select2.min.css" rel="stylesheet" />
<link href="~/Content/assets/plugins/toastr/toastr.min.css" rel="stylesheet" />

<style>
    .page-title {
        padding: 20px 36px 10px 0;
        display: block;
        position: relative;
    }

    .tbl-sm > tbody > tr > td, .tbl-sm > tbody > tr > th, .tbl-sm > thead > tr > td {
        padding: 3px 20px;
        line-height: 1.2;
        vertical-align: top;
    }
    
    .raw-msg {
        padding: 80px 0;
    }

        .raw-msg i {
            font-size: 5rem;
        }

    .raw-msg p {
            font-size: 3rem;
    }

    .slimScrollDiv {
        overflow: unset !important;
    }

    .user-block .username, .user-block .description, .user-block .comment {
        display: block;
        margin-left: 5px;
    }

    .box .nav-stacked > li:last-child {
        border-bottom: 1px solid #f4f4f4;
        margin: 0;
    }

    .subtitle {
        font-size: 12px;
    }

    .nav-stacked > li {
        cursor: pointer;
    }


    .nav-tabs.nav-tabs-bottom > li.active > a:after {
        /*background-color: #f06292;*/
        background-color: #2196f3;
    }

    .checker input[type=checkbox], .choice input[type=radio] {
        border: none;
        background: 0 0;
        display: -moz-inline-box;
        display: inline-block;
        margin: 0;
        vertical-align: top;
        cursor: pointer;
        position: absolute;
        top: -2px;
        left: -2px;
        z-index: 2;
        opacity: 0;
    }

    .checkbox-inline, .radio-inline {
        position: relative;
        padding-left: 28px;
    }

    .checkbox-inline, .radio-inline {
        position: relative;
        display: inline-block;
        padding-left: 20px;
        margin-bottom: 0;
        vertical-align: middle;
        font-weight: 400;
        cursor: pointer;
    }
</style>



<div ng-controller="ComptAssmntTool">
    <!-- Page header -->
    <div class="page-header">
        <div class="page-header-content">
            <div class="page-title">
                <h4><i class="icon-books"></i> <span class="text-semibold">COMPETENCY ASSESSMENT TOOL</span>  </h4>
            </div>
            <div class="heading-elements">

            </div>
        </div>

        <!-- Toolbar -->
        <div class="navbar navbar-default navbar-component navbar-xs">
            <ul class="nav navbar-nav visible-xs-block">
                <li class="full-width text-center"><a data-toggle="collapse" data-target="#navbar-filter"><i class="icon-menu7"></i></a></li>
            </ul>
            <div class="navbar-collapse collapse" id="navbar-filter">
                <ul class="nav navbar-nav">

                    <li class="active"><a href="#standard" data-toggle="tab"><i class="icon-podium position-left"></i>Competency Standard</a></li>
                    <li class=""><a href="#comptTool" data-toggle="tab" ng-click="loadCompetencyByGroup(1)"><i class="icon-pencil-ruler position-left"></i>Competency Tool</a></li>
                    <li class=""><a href="#departmentPosition" ng-click="viewPlantillaPosition()" data-toggle="tab"><i class="icon-align-center-horizontal position-left"></i> Position Mapping</a></li>
                </ul>
                <div class="navbar-right">
                    <ul class="nav navbar-nav">

                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown"> <i class="icon-gear"></i> <span class="visible-xs-inline-block position-right"> Options</span> <span class="caret"></span></a>
                            <ul class="dropdown-menu dropdown-menu-right">

                                <li>
                                    <a href="#" ng-click="modalSelectDepartment()" data-toggle="tab"><i class="icon-align-center-horizontal position-left"></i> Select Department</a>
                                </li>

                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <!-- /toolbar -->
    </div>

    <!-- Content area -->
    <div class="content">
        <!-- User profile -->
        <div class="row">

            <div class="col-lg-12">
                <div class="tabbable">
                    <div class="tab-content">

                        <!-- COMPETENCY STANDARD -->
                        <div class="tab-pane fade in active" id="standard">
                            <!-- Profile info -->
                            <div class="panel panel-flat">
                                <div class="panel-heading">
                                    <h5 class="panel-title">
                                        <span class="text-blue-800">{{departmentData.shortDepartmentName}}</span>  PROFICIENCY STANDARD
                                    </h5>

                                </div>
                                <div class="panel-body">

                                    <div class="row animated fadeInDown">
                                        <div class="col-md-4">

                                            <div class="box box-primary" style="">
                                                <div class="box-header with-border">
                                                    <label style="font-size:13px;" class="box-title"><b>POSITION LISTS</b></label>
                                                </div>
                                                <div class="mailbox-controls">
                                                    <div class="input-group">
                                                        <input ng-model="query" type="text" class="form-control" placeholder="Search Positions">
                                                        <span class="input-group-btn">
                                                            <button type="submit" name="search" id="search-btn" class="btn btn-flat">
                                                                <i class="fa fa-search"></i>
                                                            </button>
                                                        </span>
                                                    </div>
                                                </div>
                                                <!-- /.box-header -->
                                                <div class="box-body no-padding" style="height: 450px; overflow: scroll">
                                                    <section id="list-limiter">
                                                        <ul class="nav nav-pills nav-stacked">
                                                            <li ng-repeat="item in comptPositionList | filter:query"
                                                                ng-click="loadComptPositionStandard(item);"
                                                                ng-class="{'active': item.comptPositionCode == comptPositionCode}" style="padding:0px;">
                                                                <a style="margin:0px; padding:6px;">
                                                                    <div class=" user-block" style="width:100%;">

                                                                        <label style="font-size:11px; padding:0px; margin:0px; font-weight:normal;">{{$index+1}}. {{item.comptPositionTitle.toUpperCase()}} <span ng-if="item.subPositionTitle.length >0" class="text-muted">({{item.comptPositionTitleSub}})</span></label>

                                                                        <div class="pull-right">

                                                                        </div>

                                                                    </div>
                                                                </a>
                                                            </li>
                                                        </ul>
                                                    </section>
                                                </div>
                                            </div>
                                        </div>


                                        <div class="col-md-8">

                                            <div class="panel panel-flat">


                                                <div class="panel-body">

                                                    <div style="width:100%;">
                                                        <label class="label label-primary" ng-if="positionData.comptPositionTitle.length > 0" style="font-size:1.2em;">
                                                            {{positionData.comptPositionTitle.toUpperCase()}}
                                                        </label>
                                                    </div>

                                                    <div class="tabbable">

                                                        <div class="text-center animated fadeInUp block-message col-md-12" ng-hide="positionData.comptPositionTitle.length" ng-cloak>
                                                            <h4 class="text-blue">
                                                                <i class="icon-info3"></i>
                                                                No Position Selected!
                                                            </h4>
                                                        </div>

                                                        <ul ng-if="positionData.comptPositionTitle.length" class="nav nav-tabs nav-tabs-bottom" style="font-weight:bold;   ">

                                                            <li ng-click="changeTab(1)" ng-class="{'active':  tabNo == 1 }"><a href="#bottom-tab1" data-toggle="tab">&nbsp; CORE &nbsp; </a></li>
                                                            <li ng-click="changeTab(2)" ng-class="{'active':  tabNo == 2 }"><a href="#bottom-tab2" ng-if="myLeadTag" data-toggle="tab">LEADERSHIP</a></li>
                                                            <li ng-click="changeTab(3)" ng-class="{'active':  tabNo == 3 }"><a href="#bottom-tab3" ng-if="myTechTag" data-toggle="tab">TECHNICAL</a></li>

                                                            <li class="pull-right" ng-if="positionData.comptPositionTitle.length > 0">

                                                                <a href="#" class="btn btn-default dropdown-toggle btn-xs" data-toggle="dropdown">
                                                                    <span class="pull-right  text-blue-600 text-bold text-size-mini " style="cursor:pointer; text-transform:uppercase; padding-top:5px;">
                                                                        <i class="icon icon-gear"></i> Option
                                                                    </span>
                                                                </a>
                                                                <ul class="dropdown-menu" style="font-size:.95em; font-weight:bold">
                                                                    <li><a href="#" style="padding:3px 10px;"> <span class="icon icon-check text-info"></span> Core</a></li>
                                                                    <li>
                                                                        <a href="#" style="padding:3px 10px;" ng-click="openModalLeadership()">
                                                                            <span ng-if="myLeadTag == true" class="icon icon-check text-info"></span>
                                                                            <span ng-if="myLeadTag == false" class="icon icon-check text-muted"></span>
                                                                            Leadership
                                                                        </a>
                                                                    </li>
                                                                    <li>
                                                                        <a href="#" style="padding:3px 10px;  " ng-click="openTechComptList()">
                                                                            <span ng-if="myTechTag == true" class="icon icon-check text-info"></span>
                                                                            <span ng-if="myTechTag == false" class="icon icon-check text-muted"></span>
                                                                            Select Technical
                                                                        </a>
                                                                    </li>

                                                                </ul>
                                                            </li>

                                                        </ul>
                                                        @*Tab #1 CORE*@
                                                        <div class="tab-content" ng-if="positionData.comptPositionTitle.length > 0">
                                                            <div class="tab-pane active" ng-if="tabNo == 1">
                                                                <section class="invoice" id="title-limiterC">

                                                                    <table class="table table-striped">
                                                                        <thead style="">
                                                                            <tr>
                                                                                <td>Competencies</td>
                                                                                <td style="width:200px!important;">Standard</td>
                                                                            </tr>
                                                                        </thead>

                                                                        <tbody style="margin-top:2%;">
                                                                            <tr ng-repeat="c in coreTitle">
                                                                                <td>{{$index+1}}. {{c.comptName}}</td>
                                                                                <td>
                                                                                    <select id="" class="form-control readonly"
                                                                                            ng-model="standardID[c.comptCode]"
                                                                                            ng-change="competencyStandard(standardID[c.comptCode],c.comptCode);"
                                                                                            ng-options="x.value as x.standardName for x in standards" style="width:100%;" @*disabled="disabled"*@></select>
                                                                                </td>
                                                                            </tr>
                                                                        </tbody>
                                                                    </table>

                                                                </section><br />
                                                                <small style="margin-left:3%;"><code>Note: Select From Dropdown to Set Standard</code></small>

                                                            </div>
                                                            @*Tab #2 Leadership*@
                                                            <div ng-if="tabNo == 2">
                                                                <section class="invoice" id="title-limiterL">

                                                                    <table class="table table-striped">
                                                                        <thead style="">

                                                                            <tr>
                                                                                <td>Competencies</td>
                                                                                <td style="width:200px!important;">Standard</td>
                                                                            </tr>
                                                                        </thead>
                                                                        <tbody style="margin-top:2%;">
                                                                            <tr ng-repeat="c in leadTitle">
                                                                                <td>{{$index+1}}. {{c.comptName}}</td>
                                                                                <td style="">
                                                                                    <select class="form-control "
                                                                                            ng-model="standardID[c.comptCode]"
                                                                                            ng-change="competencyStandard(standardID[c.comptCode],c.comptCode);"
                                                                                            ng-options="x.value as x.standardName for x in standards" style="width:100%; " @*disabled="disabled"*@></select>
                                                                                </td>
                                                                            </tr>
                                                                        </tbody>
                                                                    </table>
                                                                </section><br />
                                                                <small style="margin-left:3%;"><code>Note: Select From Dropdown to Set Standard</code></small>
                                                            </div>
                                                            @*Tab #3 Technical*@
                                                            <div ng-if="tabNo == 3">

                                                                <section class="invoice" id="title-limiterC">

                                                                    <table class="table table-striped">
                                                                        <thead style="">

                                                                            <tr>
                                                                                <td>Competencies</td>
                                                                                <td style="width:200px!important;">Standard</td>
                                                                            </tr>
                                                                        </thead>

                                                                        <tbody style="margin-top:2%;">
                                                                            <tr ng-repeat="c in techTitle">
                                                                                <td>{{$index+1}}. {{c.comptName}} </td>
                                                                                <td>
                                                                                    <select id="" class="form-control"
                                                                                            ng-model="standardID[c.comptCode]"
                                                                                            ng-change="competencyStandard(standardID[c.comptCode],c.comptCode);"
                                                                                            ng-options="x.value as x.standardName for x in standards" style="width:100%;"></select>
                                                                                </td>
                                                                            </tr>
                                                                        </tbody>

                                                                    </table>

                                                                </section><br />

                                                                <small style="margin-left:3%;"><code>Note: Select From Dropdown to Set Standard</code></small>

                                                            </div>

                                                        </div>

                                                    </div>
                                                </div>
                                            </div>

                                        </div>

                                    </div>


                                </div>
                            </div>

                        </div>


                        <!-- COMPETENCY TOOL -->
                        <div class="tab-pane fade in" id="comptTool">
                            <!-- Profile info -->

                            <div class="panel panel-flat">
                                <div class="panel-heading">
                                    <h5 class="panel-title">
                                        <span class="text-blue-800">{{departmentData.shortDepartmentName}} </span>  COMPETENCY TOOL 
                                    </h5>

                                </div>



                                <div class="panel-body" ng-cloak style="min-height:450px;">

                                    <div class="row ">

                                        <div class="col-md-3">
                                            <div class="box box-primary" style="">
                                                <div class="box-header with-border" style="border-bottom: 1px solid #367fa9;">
                                                    <label style="font-size:13px;  cursor:pointer;" class="box-title"><b>LIST OF <span class="text-blue-600" ng-click="selectToolGroup()">{{toolGroup}}</span> COMPETENCIES</b> </label> &nbsp;
                                                    <span class="icon-stack-plus text-blue-600 cursor-pointer" data-toggle="tooltip" title="Add Competency" ng-click="addCompetency()"></span>
                                                </div>

                                                <!-- /.box-header -->
                                                <div class="box-body no-padding">
                                                    <div id="list-limiter">
                                                        <ul class="nav nav-pills nav-stacked">
                                                            <li ng-repeat="of in comptList | orderBy: 'orderNo'"
                                                                ng-click="loadIndicators(of)"
                                                                ng-class="{'active': of.comptCode == comptCode}">
                                                                <a>
                                                                    <div class="user-block">
                                                                        <span class="username">
                                                                            <span class="" style="font-size:.95em; font-weight:500">
                                                                                {{$index+1}}. {{of.comptName.toUpperCase()}}
                                                                            </span>
                                                                        </span>

                                                                    </div>
                                                                </a>
                                                            </li>
                                                        </ul>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>


                                        <div class="col-md-9 ">

                                            <h5 style="margin-top:3%;" ng-if="comptList.length <= 0" class="text-center text-muted text-blue animated fadeInDown">
                                                <i class="fa fa-info-circle"></i>
                                                No Data to Show.<br /><br /> Please Click the above button <label style="color:#367fa9;">ADD COMPETENCY</label> to Add.
                                            </h5>


                                            <h5 style="margin-top:3%;" ng-if="comptList.length >= 1 && competency == 0" class="text-center text-muted text-blue animated fadeInDown">
                                                <i class="fa fa-info-circle"></i>
                                                No selected competency<br />
                                            </h5>

                                            <div class="box box-solid animated fadeInDown" ng-if="comptList.length >= 1 && competency.comptName.length > 0" ng-cloak>
                                                <div class="box-header with-border" ng-hide="edit" style="background-image:linear-gradient(to right,#a0c7ef,#fafbff);border-radius: 1px 1px; padding:5px">

                                                    <table style="width:100%;">
                                                        <tr>
                                                            <td style="width:90%">
                                                                <label class="box-title" style="padding-bottom:0px; margin-bottom:0px;">
                                                                    <span>
                                                                        <b style="text-decoration:underline;color: #134f7b;">{{competency.comptName.toUpperCase()}}</b> <i class="icon-pen text-blue-800" style="font-size:16px; cursor:pointer" data-toggle="tooltip" title="Click to edit competency" ng-click="editCompetency(competency.comptCode)"></i>
                                                                    </span>
                                                                </label>
                                                                <p style="padding:0px; margin:0px;">
                                                                    <span style="font-size:13px;color: #314e5d;">
                                                                        {{competency.comptDesc}}
                                                                    </span>
                                                                </p>

                                                            </td>
                                                            <td style="text-align:center; width:10%;">
                                                                <i class="icon-stack-plus text-blue-800" style="font-size:16px; cursor:pointer" data-toggle="tooltip" title="Click to add main indicator" ng-if="comptTool == '3' " ng-click="addBehaviorMain(competency.comptCode)"></i>
                                                            </td>
                                                        </tr>
                                                    </table>
                                                </div>


                                                <div class="box-body" ng-if="comptTool == '3'">
                                                    <div class="box-group" id="accordion" ng-repeat="i in KBIList">
                                                        <div class="panel box box-primary">
                                                            <div class="box-header with-border">

                                                                <table style="width:100%; padding:5px;">
                                                                    <tr>
                                                                        <td style="padding:10px 0px;">
                                                                            <a data-toggle="collapse" style="font-size:15px;  " data-parent="#accordion" href="#{{}}">
                                                                                &nbsp;  {{i.orderNo}}. {{i.KBI | uppercase}} &nbsp;
                                                                                <i class="icon-pen text-blue-800" style="font-size:14px;" data-toggle="tooltip" title="Click to edit main indicator" ng-click="editIndicatorMain(i.KBICode)"></i>
                                                                            </a>
                                                                        </td>
                                                                        <td style="width:10%; text-align:center;">
                                                                            <i class="icon-stack-plus text-blue-800" style="font-size:14px; cursor:pointer" data-toggle="tooltip" title="Click to add indicator" ng-click="addIndicator(i.KBICode)"></i>
                                                                        </td>
                                                                    </tr>
                                                                </table>

                                                            </div>
                                                            <div id="collapseOne" class="panel-collapse collapse in">
                                                                <div class="box-body" style="padding:5px;">
                                                                    <table class="table table-hover" ng-hide="e2Sub">
                                                                        <tbody ng-repeat="sub in i.list">
                                                                            <tr>
                                                                                <td>
                                                                                    {{i.orderNo}}.{{sub.orderNo}}. {{sub.KBI}}
                                                                                    <i class="icon-pen text-blue-800" style="font-size:10px; cursor:pointer" data-toggle="tooltip" title="Click to edit indicator" ng-click="editIndicator(sub.KBICode)"></i>
                                                                                </td>

                                                                            </tr>
                                                                        </tbody>
                                                                    </table>

                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="box box-solid" ng-if="comptTool == '1' || comptTool == '2'">
                                                    <table class="table table-bordered table-hover">
                                                        <thead>
                                                            <tr>
                                                                <th ng-if="comptTool == '2'"><b>Level</b></th>
                                                                <th><b>Basic</b></th>
                                                                <th><b>Intermediate</b></th>
                                                                <th><b>Advanced</b></th>
                                                                <th><b>Superior</b></th>
                                                                <th colspan="2" class="text-center"><button class="btn btn-info btn-xs" ng-click="addKBIProg(competency.comptCode)"><i class="fa fa-plus-circle"></i> Add   </button></th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="i in KBIProg">
                                                                <td style="font-size:13px;" ng-if="comptTool == '2'">{{i.comptLevel}}</td>
                                                                <td style="font-size:13px;">{{i.basic}}</td>
                                                                <td style="font-size:13px;">{{i.intermediate}}</td>
                                                                <td style="font-size:13px;">{{i.advance}}</td>
                                                                <td style="font-size:13px;">{{i.superior}}</td>

                                                                <td class="text-center" style="width:90px!important">
                                                                    <i class="icon-pen2 text-blue-800" style="font-size:14px; cursor:pointer " ng-click="editKBIProg(i.KBICode)" data-toggle="tooltip" title="Click to Edit"></i>
                                                                </td>

                                                            </tr>
                                                        </tbody>
                                                    </table>

                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                </div>


                            </div>


                        </div>



                        <!-- PLANTILLA POSITION MAPPING-->
                        <div class="tab-pane fade in" id="departmentPosition">

                            <div class="panel panel-flat">
                                <div class="panel-heading">
                                    <h5 class="panel-title">
                                        <span class="text-blue-800">{{departmentData.shortDepartmentName}} </span>  DEPARTMENT POSITION
                                    </h5>

                                </div>
                                <div class="panel-body">

                                    <h4 class="page-title text-danger text-bold" ng-show="isLoading == true">
                                        Wait, loading data...
                                    </h4>


                                    <table class="table table-borderless table-responsive">
                                        <tr ng-repeat="item in plantilla" class="no-margin">
                                            <td class="no-padding">
                                                <label class="text-bold page-title no-margin-top no-margin-bottom" style="margin-top:0px!important"> {{item.structureName}}</label>
                                                <table class="table table-striped table-responsive">

                                                    <tr ng-repeat="itm in item.positionList">

                                                        <td>
                                                            <span class="label label-danger" ng-show="itm.isFunded == false">{{itm.itemNo}}</span>
                                                            <span ng-show="itm.isFunded == true">{{itm.itemNo}}</span>
                                                            -
                                                            <span class="text-muted text-semibold" ng-show="itm.isFunded == false">{{itm.positionTitle}}</span>
                                                            <span ng-show="itm.isFunded == true">{{itm.positionTitle}}</span>

                                                            <span class="text-muted" ng-show="itm.isFunded == false">({{itm.salaryGrade}}/1)</span>
                                                            <span class="text-muted" ng-show="itm.isFunded == true">({{itm.salaryGrade}}/{{itm.step}})</span>

                                                        </td>

                                                        <td style="width:40%!important;">

                                                            <span class="label label-danger cursor-pointer" ng-show="itm.plantillaNo == 0" ng-click="openModalSelectCompetency(itm)"> NO COMPETENCY GROUP </span>
                                                            <span class="label label-primary bg-blue-700 cursor-pointer" ng-show="itm.plantillaNo == 1"> {{itm.fullNameLast}} </span>
                                                            <span class="pull-right text-danger-700 cursor-pointer" ng-show="itm.plantillaNo == 1" ng-disabled="bDisable" ng-click="removeComptMapping(itm)"> <i class="icon-database-remove"></i> </span>

                                                        </td>

                                                        <td class="text-center" style="width:10px!important;">
                                                            <ul class="icons-list">
                                                                <li class="dropdown">
                                                                    <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="icon-arrow-down12"></i></a>
                                                                    <ul class="dropdown-menu dropdown-menu-right">
                                                                        <li><a href="#" ng-click="PrintPreview(item.appointmentCode)"><i class="icon-user-check"></i>View Profile</a></li>
                                                                    </ul>
                                                                </li>
                                                            </ul>
                                                        </td>

                                                    </tr>
                                                </table>
                                            </td>

                                        </tr>
                                    </table>




                                </div>
                            </div>

                        </div>


                    </div>
                </div>
            </div>


        </div>

    </div>


    <!-- Modal APPOINTMENT CREATE-->
    <div class="modal fade" id="modalAppointment" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="form-section" style="margin-top:0px; padding-top:0px;"><i class="icon-clipboard3"></i>  <span class="text-bold text-blue-700"> {{appointmentData.fullName}}  </span> </h5>
                </div>
                <div class="modal-body">

                    <div class="form-group">
                        <div class="row">

                            <div class="col-md-6">
