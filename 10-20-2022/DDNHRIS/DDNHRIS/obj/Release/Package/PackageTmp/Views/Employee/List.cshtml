@{
    ViewBag.Title = "List";
}

<link href="~/Content/assets/plugins/toastr/toastr.min.css" rel="stylesheet" />
<link href="~/Content/assets/plugins/select2/select2.min.css" rel="stylesheet" />
<link href="~/Content/assets/plugins/swal/sweetalert.css" rel="stylesheet" />

<style>
    .page-title {
        padding: 20px 36px 10px 0;
        display: block;
        position: relative;
    }

    .table > tbody > tr > td, .table > tbody > tr > th, .table > tfoot > tr > td, .table > tfoot > tr > th, .table > thead > tr > td, .table > thead > tr > th {
        padding: 8px 20px;
        line-height: 1.5384616;
        vertical-align: top;
        border-top: 1px solid #ddd;
    }
</style>

<div ng-controller="EmployeeList">
    <!-- Page header -->
    <div class="page-header">
        <div class="page-header-content">
            <div class="page-title">
                <h4><i class="icon-users"></i> <span class="text-semibold">EMPLOYEE LIST</span> </h4>
            </div>
        </div>
        <div class="breadcrumb-line breadcrumb-line-component" ng-show="tabId == 1">
            <ul class="breadcrumb">
                <li><a href="~/Home/Dashboard"><i class="icon-home2 position-left"></i> Home</a></li>
                <li><a href="#">Employee</a></li>
                <li class="active">List</li>
            </ul>
            <ul class="breadcrumb-elements">

                <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                        <i class="icon-gear position-left"></i>
                        Option
                        <span class="caret"></span>
                    </a>
                    <ul class="dropdown-menu dropdown-menu-right">
                        <li ng-click="showRegistrationModal()"><a href="#"><i class="icon-user-check"></i> Registration</a></li>
                        <li ng-click="showModalByCharges()"><a href="#"><i class="icon-file-check"></i> By Charges</a></li>
                        <li ng-click="showModalWorkGroup()"><a href="#"><i class="icon-file-check"></i> By Warm Bodies</a></li>
                        <li class="divider"></li>
                        <li ng-click="openMigrationModal()"><a href="#"><i class="icon-database-arrow"></i> Data Migration</a></li>
                        <li><a href="#"><i class="icon-box"></i> Archive Record</a></li>
                    </ul>
                </li>

            </ul>
        </div>

        <!-- Toolbar -->
        <div class="navbar navbar-default navbar-component navbar-xs" ng-show="tabId == 2">
            <ul class="nav navbar-nav visible-xs-block">
                <li class="full-width text-center"><a data-toggle="collapse" data-target="#navbar-filter"><i class="icon-menu7"></i></a></li>
            </ul>
            <div class="navbar-collapse collapse" id="navbar-filter">
                <ul class="nav navbar-nav">
                    <li ng-click="loadProfile()" class="active"><a href="#profile" data-toggle="tab"><i class="icon-user-check position-left"></i> Profile Info</a></li>
                    <li ng-click="loadFamily()"><a href="#family" data-toggle="tab"><i class="icon-users4 position-left"></i> Family </a></li>
                    <li ng-click="showEducation()"><a href="#education" data-toggle="tab"><i class="icon-graduation position-left"></i> Education</a></li>
                    <li ng-click="eligibilityProfile()"><a href="#serviceEligibility" data-toggle="tab"><i class="icon-certificate position-left"></i> Eligibility</a></li>
                    <li><a href="#workexperience" data-toggle="tab"><i class="icon-file-word position-left"></i> Work Experience</a></li>
                    <li ng-click="showTraining()"><a href="#trainings" data-toggle="tab"><i class="icon-calendar3 position-left"></i> Trainings </a></li>
                </ul>
                <div class="navbar-right">
                    <ul class="nav navbar-nav">
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="icon-gear"></i> <span class="visible-xs-inline-block position-right"> Options</span> <span class="caret"></span></a>
                            <ul class="dropdown-menu dropdown-menu-right">
                                <li><a href="#" ng-click="printPDS(1)"><i class="icon-file-check"></i> Personal Data Sheet </a></li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
        </div>

    </div>
    <!-- /page header -->
    <!-- Content area -->
    <div class="content" ng-show="tabId == 1">
        <!-- User profile -->
        <div class="row">

            <div class="col-lg-12">
                <div class="tabbable">
                    <div class="tab-content">

                        <!-- APPOINTMENT -->
                        <div class="tab-pane fade in active" id="list">

                            <!-- Task manager table -->
                            <div class="panel panel-white">
                                <div class="panel-heading no-padding-bottom">

                                    <div class="input-group content-group" style="width:400px; padding-top:15PX;">
                                        <div class="has-feedback has-feedback-left">
                                            <input type="text" class="form-control" placeholder="Search" ng-model="searchText">
                                            <div class="form-control-feedback">
                                                <i class="icon-search4 text-muted text-size-base"></i>
                                            </div>
                                        </div>
                                        <div class="input-group-btn" ng-disabled="bDisable">
                                            <button type="button" class="btn btn-primary" ng-click="searchEmployee()">Search</button>
                                        </div>
                                    </div>

                                </div>
                                <table class="table tasks-list table-striped ">

                                    <thead>
                                        <tr class="text-size-large">
                                            <th style="width:30px!important">#</th>
                                            <th>ID NO.</th>
                                            <th>EMPLOYEE NAME</th>
                                            <th>POSITION TITLE</th>
                                            <th class="text-center">STATUS</th>
                                            <th class="text-center">SALARY</th>
                                            <th>WORK GROUP</th>
                                            <th class="text-center"></th>
                                        </tr>
                                    </thead>
                                    <tbody>

                                        <tr ng-repeat="item in empList | filter :searchString">
                                            <td>{{$index+1}}</td>
                                            <td>
                                                {{('0000' + item.idNo).slice(-4) }}
                                            </td>
                                            <td>
                                                <span ng-click="viewProfile(item)" class="  text-blue-800 text-semibold cursor-pointer"> {{item.fullNameLast}}</span>
                                            </td>
                                            <td>
                                                {{item.positionTitle}} <span class="text-muted text-semibold" ng-if="item.salaryGrade >= 1"> ({{item.salaryGrade}}/{{item.step}})  </span>
                                            </td>
                                            <td class="text-center">
                                                {{item.employmentStatusTag}}
                                            </td>
                                            <td class="text-right">
                                                {{item.salaryRate | number:2}}/{{item.salaryType}}
                                            </td>
                                            <th>
                                                <span class="text-blue-800 text-semibold cursor-pointer" ng-if="item.workGroupName != null" ng-click="selectWorkGroup(item)">{{item.workGroupName}}</span>
                                                <span ng-if="item.workGroupName == null" ng-click="selectWorkGroup(item)"><label class="label label-danger cursor-pointer">-- NONE --</label></span>

                                                <span class="text-danger-300 pull-right" ng-show="item.tag == 0"> <i class="icon-exclamation"></i> </span>

                                            </th>
                                            <td class="text-center">
                                                <ul class="icons-list">
                                                    <li class="dropdown">
                                                        <a href="#" class="dropdown-toggle text-blue-800" data-toggle="dropdown">
                                                            <i class="icon-arrow-down32"></i>
                                                        </a>
                                                        <ul class="dropdown-menu dropdown-menu-right">
                                                            <li ng-click="viewProfile(item)"><a href="#"><i class="icon-profile"></i> View Profile</a></li>
                                                            <li ng-click="viewServiceRecord(item)"><a href="#"><i class="icon-book"></i> Service Record</a></li>
                                                            <li ng-click="sendToArchive(item)"><a href="#"><i class="icon-drawer-in"></i> Archive Record </a>  </li>

                                                        </ul>
                                                    </li>
                                                </ul>
                                            </td>
                                        </tr>

                                    </tbody>
                                </table>

                            </div>
                            <!-- /task manager table -->

                        </div>

                    </div>
                </div>
            </div>


        </div>
    </div>

    <!-- Content area -->
    <div class="content" ng-show="tabId == 2">
        <!-- User profile -->
        <div class="row">
            <div class="col-lg-10">
                <div class="tabbable">
                    <div class="tab-content">

                        <!-- PROFILE -->
                        <div class="tab-pane fade in active" id="profile">
                            <!-- Profile info -->
                            <div class="panel panel-flat">
                                <div class="panel-heading">
                                    <h5 class="content-group text-semibold no-padding-top no-padding-bottom ">
                                        <i class="icon-arrow-left52 text-blue-800 cursor-pointer" ng-click="backToList()"></i>   PROFILE INFORMATION

                                        <span class="pull-right" ng-click="enableText(1)" ng-hide="isReadOnly == false ">

                                        </span>

                                    </h5>

                                </div>
                                <div class="panel-body">

                                    <div class="form-group">
                                        <div class="row">

                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">Last Name</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.lastName" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">First Name</label>
                                                <input id="text" ng-readonly="isReadOnly" type="text" ng-model="profileData.firstName" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">Middle Name</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.middleName" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">Name Extension</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.extName" placeholder="Jr./Sr." class="form-control text-semibold">
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <div class="row">
                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">Name Prefix <span class="text-danger">*</span></label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.namePrefix" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">Name Suffix</label>
                                                <input id="text" ng-readonly="isReadOnly" type="text" ng-model="profileData.nameSuffix" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">Sex</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.sex" ng-if="isReadOnly == true" class="form-control text-semibold">
                                                <select ng-if="isReadOnly ==  false" class="form-control select2 select-Custom text-semibold" ng-model="profileData.sex" style="width:100%;"
                                                        ng-options="c.code as c.name for c in sexList" value="{{c.code}}"
                                                        id="sex">
                                                    <option value="" selected="">-- Select --</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>

                                    @*<hr />*@
                                    <div class="form-group">
                                        <div class="row">
                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">Date of Birth</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-if="isReadOnly == true" ng-model="formatDate(profileData.birthDate)" class="form-control text-semibold">
                                                <input type="date" ng-model="profileData.birthDate" ng-if="isReadOnly == false" class=" form-control text-semibold ">

                                            </div>
                                            <div class="col-md-9">
                                                <label class="text-grey-600 no-margin-bottom">Place of Birth</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.birthPlace" placeholder="" class="form-control text-semibold">
                                            </div>

                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <div class="row">
                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">Civil Status  </label>
                                                <input type="text" ng-model="profileData.civilStatCode" ng-readonly="isReadOnly" ng-if="isReadOnly == true" class="form-control text-semibold">
                                                <select ng-if="isReadOnly == false" class="form-control select2 select-Custom text-semibold" ng-model="profileData.civilStatCode" style="width:100%;"
                                                        ng-options="c.civilStatCode as c.civilStatName for c in civilStatus" value="{{c.civilStatCode}}"
                                                        id="warmBody">
                                                    <option value="" selected="">-- Select --</option>
                                                </select>
                                            </div>
                                            <div class="col-md-2">
                                                <label class="text-grey-600 no-margin-bottom">Height</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.height" placeholder="(In Meters)" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-2">
                                                <label class="text-grey-600 no-margin-bottom">Weight</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.weight" class="form-control text-semibold">
                                            </div>

                                            <div class="col-md-2">
                                                <label class="text-grey-600 no-margin-bottom">Blood Type</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.bloodType" class="form-control text-semibold">
                                            </div>



                                        </div>
                                    </div>


                                    <div class="form-group">
                                        <div class="row">

                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">GSIS ID No</label>
                                                <input type="text" ng-readonly="true" ng-model="profileData.GSISIDNo" class="form-control text-semibold">
                                            </div>

                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">GSIS BP No</label>
                                                <input type="text" ng-readonly="true" ng-model="profileData.BPNo" class="form-control text-semibold">
                                            </div>

                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">HDMF MID</label>
                                                <input type="text" ng-readonly="true" ng-model="profileData.HDMFMID" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">PHILHEALTH NO.</label>
                                                <input type="text" ng-readonly="true" ng-model="profileData.PHICNo" placeholder="" class="form-control text-semibold">
                                            </div>


                                        </div>
                                    </div>


                                    <div class="form-group">
                                        <div class="row">

                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">SSS NO.</label>
                                                <input type="text" ng-readonly="true" ng-model="profileData.SSSNo" class="form-control text-semibold">
                                            </div>

                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">TIN </label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.TINNo" class="form-control text-semibold">
                                            </div>

                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">Agency ID No. </label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.idNo" class="form-control text-semibold  text-center">
                                            </div>


                                        </div>
                                    </div>




                                    <div class="form-group">

                                        <div class="row">
                                            <div class="col-md-3">
                                                <label class="text-muted no-margin-bottom">TELEPHONE NO.</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.telephoneNo" value="Kopyov" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="text-muted no-margin-bottom">MOBILE NO</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.mobileNo" placeholder="" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-4">
                                                <label class="text-muted no-margin-bottom">EMAIL ADDRESS</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.emailAddress" placeholder="" class="form-control text-semibold">
                                            </div>

                                        </div>
                                    </div>


                                    <h6 class="panel-title text-blue-700" style="border-bottom:solid 2px #dedede; width:100%; padding-top:10px;  margin:15px 0px;">RESIDENTIAL ADDRESS</h6>


                                    <div class="form-group">

                                        <div class="row">
                                            <div class="col-md-3">
                                                <label class="text-muted no-margin-bottom">House/Block/Lot No.</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.houseBlockNoResAddress" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="text-muted no-margin-bottom">Street</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.streetResAddress" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="text-muted no-margin-bottom">Subdivision/Village</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.subdivisionResAddress" class="form-control text-semibold">
                                            </div>

                                        </div>
                                    </div>


                                    <div class="form-group">

                                        <div class="row">
                                            <div class="col-md-3">
                                                <label class="text-muted no-margin-bottom">Barangay</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.brgyResAddress" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="text-muted no-margin-bottom">City/Municipality</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.cityMunResAddress" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-4">
                                                <label class="text-muted no-margin-bottom">Province</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.provinceResAddress" class="form-control text-semibold">
                                            </div>

                                            <div class="col-md-2">
                                                <label class="text-muted no-margin-bottom">Zip Code</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.ZIPResAddress" class="form-control text-semibold">
                                            </div>

                                        </div>
                                    </div>


                                    <h6 class="panel-title text-blue-700" style="border-bottom:solid 2px #dedede; width:100%; padding-top:10px; margin:15px 0px;">PERMANENT ADDRESS</h6>

                                    <div class="form-group">

                                        <div class="row">
                                            <div class="col-md-3">
                                                <label class="text-muted no-margin-bottom">House/Block/Lot No.</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.houseBlockNoPermAddress" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="text-muted no-margin-bottom">Street</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.streetPermAddress" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="text-muted no-margin-bottom">Subdivision/Village</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.subdivisionPermAddress" class="form-control text-semibold">
                                            </div>


                                        </div>
                                    </div>


                                    <div class="form-group">

                                        <div class="row">
                                            <div class="col-md-3">
                                                <label class="text-muted no-margin-bottom">Barangay</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.brgyPermAddress" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="text-muted no-margin-bottom">City/Municipality</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.cityMunPermAddress" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-4">
                                                <label class="text-muted no-margin-bottom">Province</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.provincePermAddress" class="form-control text-semibold">
                                            </div>

                                            <div class="col-md-2">
                                                <label class="text-muted no-margin-bottom">Zip Code</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.ZIPPermAddress" class="form-control text-semibold">
                                            </div>

                                        </div>
                                    </div>


                                    <div class="form-group">

                                        <div class="row">

                                            <div class="col-md-12">
                                                <button class="btn btn-primary" ng-click="enableText(1)" ng-hide="isReadOnly == false ">
                                                    Edit Profile
                                                </button>

                                                <button class="btn btn-default" ng-click="cancelEditing()" ng-hide="isReadOnly == true ">
                                                    Cancel
                                                </button>

                                                <button class="btn btn-primary pull-right" ng-click="updateProfile(profileData)" ng-hide="isReadOnly == true ">
                                                    Update Profile
                                                </button>

                                            </div>

                                        </div>
                                    </div>


                                </div>
                            </div>

                        </div>

                        <!-- FAMILY -->
                        <div class="tab-pane fade" id="family">
                            <!-- Profile info -->
                            <div class="panel panel-flat">
                                <div class="panel-heading">
                                    <h5 class="content-group text-semibold no-padding-top no-padding-bottom ">
                                        <i class="icon-arrow-left52 text-blue-800 cursor-pointer" ng-click="backToList()"></i> FAMILY BACKGROUND
                                    </h5>
                                </div>
                                <div class="panel-body">

                                    <h6 class="panel-title text-blue-800 text-semibold" style="border-bottom:solid 2px #dedede; margin-bottom:15px; padding:5px; width:100%; ">SPOUSE INFORMATION</h6>

                                    <div class="form-group">
                                        <div class="row">
                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">Surname</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="famData.spouseLastName" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">First Name</label>
                                                <input id="text" ng-readonly="isReadOnly" type="text" ng-model="famData.spouseFirstName" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">Middle Name</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="famData.spouseMiddleName" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-2">
                                                <label class="text-grey-600 no-margin-bottom">Name Extension</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="famData.spouseExtnName" placeholder="Jr./Sr." class="form-control text-semibold">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div class="row">
                                            <div class="col-md-6">
                                                <label class="text-grey-600 no-margin-bottom">Occupation</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="famData.spouseOccupation" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-6">
                                                <label class="text-grey-600 no-margin-bottom">Employeer/Business Name</label>
                                                <input id="text" ng-readonly="isReadOnly" type="text" ng-model="famData.spouseEmployeerName" class="form-control text-semibold">
                                            </div>

                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div class="row">

                                            <div class="col-md-9">
                                                <label class="text-grey-600 no-margin-bottom">Employeer/Business Address</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="famData.spouseEmployeerAddrs" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">Tel No</label>
