@{
    ViewBag.Title = "Application";
}
<link href="~/Content/assets/css/icons/icomoon/styles.css" rel="stylesheet" />
<link href="~/Content/assets/css/colors.css" rel="stylesheet" />
<link href="~/Content/assets/css/core.css" rel="stylesheet" />
<link href="~/Content/assets/css/components.css" rel="stylesheet" />

<link href="~/Content/assets/plugins/select2/select2.min.css" rel="stylesheet" />
<link href="~/Content/assets/plugins/toastr/toastr.min.css" rel="stylesheet" />
<link href="~/Content/assets/css/comptasstmntUI.css" rel="stylesheet" />
<style>
    .page-title {
        padding: 20px 36px 10px 0;
        display: block;
        position: relative;
    }

    .tbl-sm > tbody > tr > td, .tbl-sm > tbody > tr > th, .tbl-sm > thead > tr > td {
        padding: 5px 20px;
        line-height: 1.5384616;
        vertical-align: top;
    }

    .tbl-xs > tbody > tr > td, .tbl-sm > tbody > tr > th, .tbl-sm > thead > tr > td {
        padding: 3px 10px;
        line-height: 1.2;
        vertical-align: top;
    }

    .option-input {
        height: 20px;
        width: 20px;
    }

    label {
        display: inline-block;
        max-width: 100%;
        margin-bottom: 0px;
        padding-bottom: 0px;
    }
</style>

<div ng-controller="ApplicantProfiling">
    <!-- Page header -->
    <div class="page-header">
        <div class="page-header-content">
            <div class="page-title">
                <h4><i class="icon-books"></i> <span class="text-semibold">APPLICANT PROFILING</span> </h4>
            </div>
            <div class="heading-elements">

            </div>
        </div>

        <!-- Toolbar -->
        <div class="navbar navbar-default navbar-component navbar-xs">
            <ul class="nav navbar-nav visible-xs-block">
                <li class="full-width text-center"><a data-toggle="collapse" data-target="#navbar-filter"><i class="icon-menu7"></i></a></li>

            </ul>
            <div class="navbar-collapse collapse" id="navbar-filter">
                <ul class="nav navbar-nav">
                    <li class="active"><a href=" #psbSchedule" data-toggle="tab"><i class="icon-calendar2 position-left"></i> PSB Schedule</a></li>
                </ul>

                <div class="navbar-right">
                    <ul class="nav navbar-nav">

                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <i class="icon-file-plus position-left"></i>
                                Add New
                                <span class="caret"></span>
                            </a>
                            <ul class="dropdown-menu dropdown-menu-right">
                                <li><a href="#" ng-click="modalOpenPSBSchedule()"><i class="icon-file-text2"></i> PSB Screening</a></li>
                                @*<li><a href="#" ng-click="createPosition()"><i class="icon-file-text2"></i> Applicant</a></li>*@
                            </ul>
                        </li>

                    </ul>
                </div>
            </div>
        </div>
        <!-- /toolbar -->
    </div>


    <!-- Content area -->
    <div class="content">
        <!-- User profile -->
        <div class="row">

            <div class="col-lg-12">
                <div class="tabbable">
                    <div class="tab-content">

                        <!-- PBS SCHEDULE LIST -->
                        <div class="tab-pane fade in active" id="psbSchedule">

                            <div class="row" ng-show="tabId == 0">
                                <div class="col-md-6" ng-repeat="item in positionList">
                                    <div class="panel border-left-lg border-left-blue-800">
                                        <div class="panel-body">
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <h6 class="no-margin-top" ng-click="showPositionData(item)"><a href="#">{{item.itemText}} - <span class="text-uppercase">{{item.positionTitle}}</span></a></h6>
                                                    <p class="mb-15">{{item.departmentName}}</p>

                                                    <a href="#" class="" ng-repeat="itm in item.applicantList"><img src="~/Content/assets/images/demo/users/NoImage.jpg" class="img-circle img-xs" alt="" data-popup="tooltip-custom" title={{itm.applicantName}} ng-click="selectApplicant(itm)"></a>
                                                     
                                                    <button class="btn btn-link" ng-click="addApplicant(item)">
                                                        <i class="icon-plus22"></i>
                                                    </button>

                                                    @*<td>Custom color</td>
                                                        <td><button type="button" class="btn btn-default btn-sm" data-popup="tooltip-custom" title="I'm a teal tooltip">Tooltip <i class="icon-play3 position-right"></i></button></td>
                                                        <td>Tooltips support custom color styling through <code>template</code> option. To use, wrap <code>.tooltip-arrow</code> and <code>.tooltip-inner</code> in the div with a custom color class</td>
                                                    *@

                                                </div>

                                            </div>
                                        </div>
                                        <div class="panel-footer panel-footer-condensed">
                                            <div class="heading-elements">
                                                <span class="heading-text">PSB Date: <span class="text-semibold">{{formatDateLong(item.PSBDate)}}</span></span>
                                                <ul class="list-inline list-inline-condensed heading-text pull-right">

                                                    <li class="dropdown">
                                                        <a href="#" class="text-default dropdown-toggle" data-toggle="dropdown">Option <span class="caret"></span></a>
                                                        <ul class="dropdown-menu dropdown-menu-right">
                                                            <li><a href="#"><i class="icon-alarm-add"></i> Check in</a></li>
                                                            <li><a href="#"><i class="icon-rotate-ccw2"></i> Re-schedule</a></li>
                                                            <li class="divider"></li>
                                                            <li><a href="#"><i class="icon-cross2"></i> Remove</a></li>
                                                        </ul>
                                                    </li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                            </div>

                            <!-- Profile info -->
                            <div class="panel panel-flat" ng-show="tabId == 1">
                                <div class="panel-heading">
                                    <h4 class="panel-title text-bold text-blue-800">
                                        @*<span class="text-semibold"> {{('00000'+ pubItemData.itemNoText).slice(-4)}}  {{pubItemData.positionTitle}}</span>*@
                                        <span class="text-semibold"> {{pubItemData.itemText}}  {{pubItemData.positionTitle}}</span>
                                    </h4>
                                    <div class="heading-elements">

                                        <button class="btn btn-link" ng-click="backToList()">
                                            <i class="icon-arrow-left16"></i> Back to List
                                        </button>

                                    </div>
                                </div>

                                <table class="table table-bordered table-striped">
                                    <thead>
                                        <tr class="text-semibold">
                                            <th class="text-center">APPLICANT NAME</th>
                                            <th class="text-center">POSITION</th>
                                            <th class="text-center">STATUS</th>
                                            <th class="text-center" style="width:90px;">
                                                <span class="text-blue-800 text-semibold cursor-pointer" ng-click="addApplicant()">
                                                    <i class="icon-plus2"></i> Add
                                                </span>
                                            </th>
                                        </tr>
                                    </thead>

                                    <tbody>
                                        <tr ng-repeat="item in applicantList">
                                            <td>
                                                <small>{{$index + 1}}.) </small>
                                                <span class="text-primary-800 text-bold cursor-pointer" ng-click="viewApplicantProfile(item.applicationCode)"> {{item.applicantNameLast}}</span>
                                                <sup ng-show="item.appTypeCode == 0"><small class="text-danger-800 text-size-small text-semibold" style="font-size:.95em;">(OUTSIDER)</small> </sup>
                                            </td>
                                            <td>
                                                <small>
                                                    <span class="text-semibold">{{item.applicantPositionTitle}} <span ng-if="item.applicantSalaryGrade >= 1">({{item.applicantSalaryGrade}} <span ng-if="item.applicantSalaryStep >= 1">{{item.applicantSalaryStep}}</span>)</span> </span>

                                                    <span class="label label-primary pull-right " ng-show="item.employmentStatusTag != null">{{item.employmentStatusTag}}</span>

                                                </small>
                                            </td>
                                            <td></td>
                                            <td>
                                                <button class="btn btn-link btn-xs text-blue-800 text-semibold no-padding no-margin" ng-click="viewApplicantProfile(item.applicationCode)">
                                                    Profile  <i class="icon-arrow-right15"></i>
                                                </button>
                                            </td>
                                        </tr>

                                    </tbody>
                                </table>

                            </div>
