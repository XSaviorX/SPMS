                                            <th class="text-center">STATUS</th>
                                            <th class="text-center" style="width:35%;">REMARKS</th>
                                            <th class="text-center text-muted" style="width: 70px;"></th>
                                        </tr>
                                    </thead>
                                    <tbody>

                                        <tr ng-repeat="item in applicationList  | filter :searchString">
                                            <td>{{$index+1}}</td>
                                            <td>
                                                <div ng-click="selectAppliedPosition(item)"><span class="text-semibold text-blue-800 cursor-pointer"> {{item.itemNo}} - {{item.positionTitle}} </span> <span> ({{item.salaryGrade}}/1)</span> </div>
