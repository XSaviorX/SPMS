@{
    ViewBag.Title = "PDS";
}

<link href="~/Content/assets/plugins/toastr/toastr.min.css" rel="stylesheet" />
<link href="~/Content/assets/plugins/select2/select2.min.css" rel="stylesheet" />

<link href="~/Content/assets/plugins/swal/sweetalert.css" rel="stylesheet" />

<style>
    .page-title {
        padding: 20px 36px 10px 0;
        display: block;
        position: relative;
    }

    .table > tbody > tr > td, .table > tbody > tr > th, .table > tfoot > tr > td, .table > tfoot > tr > th, .table > thead > tr > td, .table > thead > tr > th {
        padding: 8px 20px;
        line-height: 1.5384616;
        vertical-align: top;
        border-top: 1px solid #ddd;
    }
</style>

<div ng-controller="MyProfile">
    <!-- Page header -->
    <div class="page-header">
        <div class="page-header-content">
            <div class="page-title">
                <h4><span class="text-semibold">PERSONAL DATA SHEET (PDS)</span> </h4>
            </div>
        </div>

        <!-- Toolbar -->
        <div class="navbar navbar-default navbar-component navbar-xs">
            <ul class="nav navbar-nav visible-xs-block">
                <li class="full-width text-center"><a data-toggle="collapse" data-target="#navbar-filter"><i class="icon-menu7"></i></a></li>
            </ul>
            <div class="navbar-collapse collapse" id="navbar-filter">
                <ul class="nav navbar-nav text-semibold text-size-mini">
                    <li ng-click="loadProfile()" class="active"><a href="#profile" data-toggle="tab"><i class="icon-user-check position-left"></i>PERSONAL INFO</a></li>
                    <li ng-click="loadFamilyBackGround()"><a href="#family" data-toggle="tab"><i class="icon-users4 position-left"></i>FAMILY</a></li>
                    <li ng-click="loadEducation()"><a href="#education" data-toggle="tab"><i class="icon-graduation position-left"></i>EDUCATION</a></li>
                    <li ng-click="loadEligibility()"><a href="#serviceEligibility" data-toggle="tab"><i class="icon-certificate position-left"></i>ELIGIBILITY</a></li>
                    <li ng-click="loadWorkExperience()"><a href="#workexperience" data-toggle="tab"><i class=" icon-archive position-left"></i>EXPERIENCE</a></li>
                    <li><a href="#workexperience" data-toggle="tab"><i class="icon-bookmark position-left"></i>VOLUNTARY WORKS</a></li>
                    <li ng-click="loadPDSTraining()"><a href="#trainings" data-toggle="tab"><i class="icon-brain position-left"></i> L & D </a></li>
                    <li ng-click="loadPDSOtherInfo()"><a href="#otherInfo" data-toggle="tab"><i class="icon-calendar3 position-left"></i> OTHERS </a></li>
                </ul>
                <div class="navbar-right">
                    <ul class="nav navbar-nav">
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="icon-gear"></i> <span class="visible-xs-inline-block position-right"> Options</span> <span class="caret"></span></a>
                            <ul class="dropdown-menu dropdown-menu-right">
                                <li><a href="#" ng-click="printPDS(1)"><i class="icon-file-check"></i> PDS Page 1 </a></li>
                                <li><a href="#" ng-click="printPDS(2)"><i class="icon-file-check"></i> PDS Page 2 </a></li>
                                <li><a href="#" ng-click="printPDS(3)"><i class="icon-file-check"></i> PDS Page 3 </a></li>
                                <li><a href="#" ng-click="printPDS(4)"><i class="icon-file-check"></i> PDS Page 4 </a></li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
        </div>

    </div>
    <!-- /page header -->
    <!-- Content area -->
    <div class="content">
        <!-- User profile -->
        <div class="row">
            <div class="col-lg-12">
                <div class="tabbable">
                    <div class="tab-content">

                        <!-- PERSONAL DATA SHEET -->
                        <div class="tab-pane fade in active" id="profile">
                            <!-- Personal Info -->
                            <div class="panel panel-flat">
                                <div class="panel-heading">
                                    <h5 class="content-group text-semibold no-padding-top no-padding-bottom ">
                                        PROFILE INFORMATION
                                        <span class="btn btn-link btn-xs text-semibold pull-right" ng-click="setToEditingMode()" ng-show="isReadOnly == true ">
                                            <i class="icon-pen2"></i> Edit
                                        </span>
                                    </h5>

                                </div>
                                <div class="panel-body">

                                    <div class="form-group">
                                        <div class="row">

                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">Last Name</label>
                                                <input type="text" ng-readonly="true" ng-model="profileData.lastName" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">First Name</label>
                                                <input id="text" ng-readonly="true" type="text" ng-model="profileData.firstName" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">Middle Name</label>
                                                <input type="text" ng-readonly="true" ng-model="profileData.middleName" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">Name Extension</label>
                                                <input type="text" ng-readonly="true" ng-model="profileData.extName" placeholder="Jr./Sr." class="form-control text-semibold">
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <div class="row">
                                            <div class="col-md-8">
                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-md-4">
                                                            <label class="text-grey-600 no-margin-bottom">Name Prefix <span class="text-danger">*</span></label>
                                                            <input type="text" ng-readonly="true" ng-model="profileData.namePrefix" class="form-control text-semibold">
                                                        </div>
                                                        <div class="col-md-4">
                                                            <label class="text-grey-600 no-margin-bottom">Name Suffix</label>
                                                            <input id="text" ng-readonly="true" type="text" ng-model="profileData.nameSuffix" class="form-control text-semibold">
                                                        </div>
                                                        <div class="col-md-4">
                                                            <label class="text-grey-600 no-margin-bottom">Sex</label>
                                                            <input type="text" ng-readonly="true" ng-model="profileData.sex" class="form-control text-semibold">
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-md-4">
                                                            <label class="text-grey-600 no-margin-bottom">Date of Birth</label>
                                                            <input type="text" ng-readonly="true" ng-model="formatDate(profileData.birthDate)" class="form-control text-semibold">
                                                        </div>
                                                        <div class="col-md-8">
                                                            <label class="text-grey-600 no-margin-bottom">Place of Birth</label>
                                                            <input type="text" ng-readonly="true" ng-model="profileData.birthPlace" placeholder="" class="form-control text-semibold">
                                                        </div>


                                                    </div>
                                                </div>


                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-md-4">
                                                            <label class="text-grey-600 no-margin-bottom">Civil Status  </label>
                                                            <input type="text" ng-model="profileData.civilStatCode" ng-readonly="true" class="form-control text-semibold">
                                                        </div>
                                                        <div class="col-md-3">
                                                            <label class="text-grey-600 no-margin-bottom">Height</label>
                                                            <input type="text" ng-readonly="isReadOnly" ng-model="profileData.height" placeholder="(In Meters)" class="form-control text-semibold">
                                                        </div>
                                                        <div class="col-md-3">
                                                            <label class="text-grey-600 no-margin-bottom">Weight</label>
                                                            <input type="text" ng-readonly="isReadOnly" ng-model="profileData.weight" class="form-control text-semibold">
                                                        </div>

                                                        <div class="col-md-2">
                                                            <label class="text-grey-600 no-margin-bottom">Blood Type</label>
                                                            <input type="text" ng-readonly="isReadOnly" ng-model="profileData.bloodType" class="form-control text-semibold">
                                                        </div>

                                                    </div>
                                                </div>

                                            </div>
                                            <div class="col-md-4" style="background-color:#f1f1f1; padding-bottom:15px;">

                                                <h5>Citizenship</h5>

                                                <table style="width:100%;">
                                                    <tr>
                                                        <td>
                                                            <label class="text-semibold text-size-large">
                                                                <input type="checkbox" ng-disabled="isReadOnly" ng-checked="profileData.citizenship == 1" ng-model="profileData.citizenship" />
                                                                Filipino
                                                            </label>
                                                        </td>
                                                    </tr>

                                                    <tr>
                                                        <td>
                                                            <label class="text-semibold text-size-large" ng-readonly="isReadOnly">
                                                                <input type="checkbox" ng-disabled="isReadOnly" ng-model="checkDualCitizenship" ng-checked="onChangeCitizenshipDual()" /> Dual Citizenship
                                                            </label>
                                                        </td>
                                                    </tr>

                                                    <tr>
                                                        <td>

                                                            <div class="form-group">
                                                                <div class="col-md-1">
                                                                    &nbsp; <input type="radio" hidden="hidden" name="citizenDualType" value="0" ng-checked="profileData.citizenshipType == 0" ng-model="profileData.citizenshipType" />
                                                                </div>
                                                                <div class="col-md-10">
                                                                    <label>
                                                                        <input type="radio" name="citizenDualType" value="1" ng-disabled="!checkDualCitizenship || isReadOnly" ng-checked="profileData.citizenshipType == 1" ng-model="profileData.citizenshipType" />
                                                                        <span class="text-size-large">By Birth</span> &nbsp; &nbsp; &nbsp;
                                                                    </label>
                                                                    <label ng-disabled="!checkDualCitizenship">
                                                                        <input type="radio" name="citizenDualType" value="2" ng-disabled="!checkDualCitizenship || isReadOnly" ng-checked="profileData.citizenshipType == 2" ng-model="profileData.citizenshipType" />
                                                                        <span class="text-size-large">By Naturalization</span>
                                                                    </label>
                                                                </div>

                                                            </div>

                                                        </td>
                                                    </tr>

                                                    <tr>
                                                        <td>

                                                            <div class="form-group">
                                                                <div class="col-md-1">
                                                                    &nbsp;
                                                                </div>

                                                                <div class="col-md-10">
                                                                    <select class="form-control select2 select-Custom text-semibold text-size-mini" ng-readonly="isReadOnly" ng-model="profileData.citizenshipCountry" style="width:100%;"
                                                                            ng-options="c.text as c.text for c in countries" value="{{c.text}}" ng-disabled="!checkDualCitizenship"
                                                                            id="country">
                                                                        <option value="" selected="selected" disabled="disabled">-- Select Country --</option>
                                                                    </select>
                                                                </div>
                                                            </div>

                                                        </td>
                                                    </tr>



                                                </table>

                                            </div>
                                        </div>
                                    </div>

                                    @*<hr />*@
                                    <div class="form-group">
                                        <div class="row">

                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">GSIS ID No</label>
                                                <input type="text" ng-readonly="true" ng-model="profileData.GSISIDNo" class="form-control text-semibold">
                                            </div>

                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">GSIS BP No</label>
                                                <input type="text" ng-readonly="true" ng-model="profileData.BPNo" class="form-control text-semibold">
                                            </div>

                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">HDMF MID</label>
                                                <input type="text" ng-readonly="true" ng-model="profileData.HDMFMID" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">PHILHEALTH NO.</label>
                                                <input type="text" ng-readonly="true" ng-model="profileData.PHICNo" placeholder="" class="form-control text-semibold">
                                            </div>

                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <div class="row">

                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">SSS NO.</label>
                                                <input type="text" ng-readonly="true" ng-model="profileData.SSSNo" class="form-control text-semibold">
                                            </div>

                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">TIN </label>
                                                <input type="text" ng-readonly="true" ng-model="profileData.TINNo" class="form-control text-semibold">
                                            </div>

                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">Agency ID No. </label>
                                                <input type="text" ng-readonly="true" ng-model="profileData.idNo" class="form-control text-semibold  text-center">
                                            </div>

                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <div class="row">
                                            <div class="col-md-3">
                                                <label class="text-muted no-margin-bottom">TELEPHONE NO.</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.telephoneNo" value="Kopyov" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="text-muted no-margin-bottom">MOBILE NO</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.mobileNo" placeholder="" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-4">
                                                <label class="text-muted no-margin-bottom">EMAIL ADDRESS</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.emailAddress" placeholder="" class="form-control text-semibold">
                                            </div>

                                        </div>
                                    </div>


                                    <h6 class="panel-title text-blue-700" style="border-bottom:solid 2px #dedede; width:100%; padding-top:10px;  margin:15px 0px;">RESIDENTIAL ADDRESS</h6>


                                    <div class="form-group">

                                        <div class="row">
                                            <div class="col-md-3">
                                                <label class="text-muted no-margin-bottom">House/Block/Lot No.</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.addrsResHouseBlk" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="text-muted no-margin-bottom">Street</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.addrsResStreet" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="text-muted no-margin-bottom">Subdivision/Village</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.addrsResVillage" class="form-control text-semibold">
                                            </div>

                                        </div>
                                    </div>


                                    <div class="form-group">


                                        <div class="row">

                                            <div class="col-md-4">
                                                <label class="text-muted no-margin-bottom">Province</label>
                                                <label class="form-control text-semibold" ng-show="isReadOnly" style="background-color:#fafafa; margin-bottom:0px; padding-bottom:0px;">
                                                    {{profileData.addrsResProvince}}
                                                </label>
                                                <div ng-show="!isReadOnly" class="text-semibold">
                                                    <select class="form-control select2 select-Custom text-semibold" ng-model="profileData.provinceResCode" style="width:100%;"
                                                            ng-change="onChangeProvince(profileData.provinceResCode, 1)"
                                                            ng-options="c.provCode as c.province for c in province" value="{{c.province}}" id="provRes">
                                                        <option value="" selected="selected" disabled="disabled">-- Select Province --</option>
                                                    </select>
                                                </div>

                                            </div>
                                            <div class="col-md-3">
                                                <label class="text-muted no-margin-bottom">City/Municipality</label>
                                                <label class="form-control text-semibold" ng-show="isReadOnly" style="background-color:#fafafa; margin-bottom:0px; padding-bottom:0px;">
                                                    {{profileData.addrsResCityMun}}
                                                </label>
                                                <div ng-show="!isReadOnly" class="text-semibold">
                                                    <select class="form-control select2 select-Custom text-semibold" ng-hide="isReadOnly == false " ng-model="profileData.cityMunResCode" style="width:100%;"
                                                            ng-disabled="!profileData.provinceResCode"
                                                            ng-options="c.cityMunCode as c.cityMunName for c in cityMun  | filter: {provCode: profileData.provinceResCode}" value="{{c.cityMunName}}" id="cityMunRes">
                                                        <option value="" selected="selected" disabled="disabled">-- Select City/Mun. --</option>
                                                    </select>
                                                </div>
                                            </div>

                                            <div class="col-md-3">
                                                <label class="text-muted no-margin-bottom">Barangay </label>
                                                <label class="form-control text-semibold" ng-show="isReadOnly" style="background-color:#fafafa; margin-bottom:0px; padding-bottom:0px;">
                                                    {{profileData.addrsResBarangay}}
                                                </label>
                                                <div ng-show="!isReadOnly" class="text-semibold">
                                                    <select class="form-control select2 select-Custom text-semibold  " ng-show="isReadOnly" ng-model="profileData.addrsResBrgyCode" style="width:100%;"
                                                            ng-disabled="!profileData.cityMunResCode"
                                                            ng-options="c.brgyCode as c.brgyName for c in barangayRes | filter: { cityMunCode: profileData.cityMunResCode}" value="{{c.brgyName}}" id="brgyRes">
                                                        <option value="" selected="selected" disabled="disabled">-- Select Barangay --</option>
                                                    </select>
                                                </div>

                                            </div>

                                            <div class="col-md-2">
                                                <label class="text-muted no-margin-bottom">Zip Code</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.addrsResZip" class="form-control text-semibold">
                                            </div>

                                        </div>
                                    </div>

                                    <h6 class="panel-title text-blue-700" style="border-bottom:solid 2px #dedede; width:100%;  margin:15px 0px;">
                                        PERMANENT ADDRESS

                                        <span class="btn btn-link text-blue-800 text-semibold" style="font-size:.75em;" ng-click="copyResidentialAddress()">
                                            <i class="icon-copy"></i>Copy
                                        </span>

                                    </h6>

                                    <div class="form-group">

                                        <div class="row">
                                            <div class="col-md-3">
                                                <label class="text-muted no-margin-bottom">House/Block/Lot No.</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.addrsPermHouseBlk" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="text-muted no-margin-bottom">Street</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.addrsPermStreet" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="text-muted no-margin-bottom">Subdivision/Village</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.addrsPermVillage" class="form-control text-semibold">
                                            </div>


                                        </div>
                                    </div>


                                    <div class="form-group">

                                        <div class="row">
                                            <div class="col-md-4">
                                                <label class="text-muted no-margin-bottom">Province</label>

                                                <label class="form-control text-semibold" ng-show="isReadOnly" style="background-color:#fafafa; margin-bottom:0px;">
                                                    {{profileData.addrsPermProvince}}
                                                </label>
                                                <div ng-show="!isReadOnly" class="text-semibold">
                                                    <select class="form-control select2 select-Custom text-semibold  " ng-readonly="isReadOnly" ng-model="profileData.provincePermCode" style="width:100%;"
                                                            ng-options="c.provCode as c.province for c in province" value="{{c.province}}"
                                                            ng-change="onChangeProvince(profileData.provincePermCode, 2)"
                                                            id="provPerm">
                                                        <option value="" selected="selected" disabled="disabled">-- Select Province --</option>
                                                    </select>
                                                </div>

                                            </div>
                                            <div class="col-md-3">
                                                <label class="text-muted no-margin-bottom">City/Municipality</label>
                                                <label class="form-control text-semibold" ng-show="isReadOnly" style="background-color:#fafafa; margin-bottom:0px; padding-bottom:0px;">
                                                    {{profileData.addrsPermCityMun}}
                                                </label>
                                                <div ng-show="!isReadOnly" class="text-semibold">
                                                    <select class="form-control select2 select-Custom text-semibold  " ng-readonly="isReadOnly" ng-model="profileData.cityMunPermCode" style="width:100%;"
                                                            ng-disabled="!profileData.provincePermCode"
                                                            ng-options="c.cityMunCode as c.cityMunName for c in cityMun  | filter: {provCode: profileData.provincePermCode}" value="{{c.cityMunName}}"
                                                            id="cityMunPerm">
                                                        <option value="" selected="selected" disabled="disabled">-- Select City/Mun. --</option>
                                                    </select>
                                                </div>

                                            </div>
                                            <div class="col-md-3">
                                                <label class="text-muted no-margin-bottom">Barangay {{barangay.length}}</label>
                                                <label class="form-control text-semibold" ng-show="isReadOnly" style="background-color:#fafafa; margin-bottom:0px;">
                                                    {{profileData.addrsPermBarangay}}
                                                </label>
                                                <div ng-show="!isReadOnly" class="text-semibold">
                                                    <select class="form-control select2 select-Custom text-semibold  " ng-readonly="isReadOnly" ng-model="profileData.addrsPermBrgyCode" style="width:100%;"
                                                            ng-disabled="!profileData.cityMunPermCode"
                                                            ng-options="c.brgyCode as c.brgyName for c in barangayPerm | filter: { cityMunCode: profileData.cityMunPermCode}" value="{{c.brgyName}}"
                                                            id="brgyPerm">
                                                        <option value="" selected="selected" disabled="disabled">-- Select Barangay --</option>
                                                    </select>
                                                </div>

                                            </div>


                                            <div class="col-md-2">
                                                <label class="text-muted no-margin-bottom">Zip Code</label>
                                                <input type="text" ng-readonly="isReadOnly" ng-model="profileData.addrsPermZip" class="form-control text-semibold">
                                            </div>

                                        </div>
                                    </div>


                                    <div class="form-group">

                                        <div class="row">

                                            <div class="col-md-12">
                                                <button class="btn btn-primary" ng-click="setToEditingMode()" ng-hide="isReadOnly == false ">
                                                    Edit Profile
                                                </button>

                                                <button class="btn btn-default" ng-click="cancelEditing()" ng-hide="isReadOnly == true ">
                                                    Cancel
                                                </button>

                                                <button class="btn btn-primary pull-right" ng-click="updateProfileInfo(profileData)" ng-hide="isReadOnly == true ">
                                                    Update Profile
                                                </button>

                                            </div>

                                        </div>
                                    </div>


                                </div>
                            </div>
                        </div>

                        <!-- FAMILY -->
                        <div class="tab-pane fade" id="family">
                            <!-- Profile info -->
                            <div class="panel panel-flat">
                                <div class="panel-heading">
                                    <h5 class="content-group text-semibold no-padding-top no-padding-bottom ">
                                        FAMILY BACKGROUND
                                    </h5>
                                </div>
                                <div class="panel-body">

                                    <h6 class="panel-title text-blue-800 text-semibold" style="border-bottom:solid 2px #dedede; margin-bottom:15px; padding:5px; width:100%; ">SPOUSE INFORMATION</h6>

                                    <div class="form-group">
                                        <div class="row">
                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">Surname</label>
                                                <input type="text" ng-readonly="true" ng-model="famData.spouseLastName" class="form-control text-semibold">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="text-grey-600 no-margin-bottom">First Name</label>
