                                                        <ul class="dropdown-menu dropdown-menu-right">
                                                            <li ng-click="ViewDetail(item.appointmentCode)"><a href="#"><i class="icon-file-text2"></i> View Details</a></li>
                                                        </ul>
                                                    </li>
                                                </ul>
                                            </td>
                                        </tr>

                                    </tbody>
                                </table>
                            </div>

                            <!-- Task manager table -->
