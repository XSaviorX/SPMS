                                        </tr>
                                    </thead>
                                    <tbody>

                                        <tr ng-repeat="item in projList  | filter :searchString">
                                            <td>{{$index+1}}</td>
                                            <td>
                                                <div class="text-bold text-blue-800 cursor-pointer" ng-click="viewDetail(item.fundSourceCode)"><span >{{item.projectName}}</span></div>
                                                <div class="text-muted">{{item.departmentName}}</div>
                                            </td>
                                            <td class="text-right text-semibold">
                                                <span class="text-bold  text-size-large">{{item.branch}}</span>
                                                <br />
