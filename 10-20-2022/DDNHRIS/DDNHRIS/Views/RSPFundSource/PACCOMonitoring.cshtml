                                                            <li ng-click="viewDetail(item.fundSourceCode)"><a href="#"><i class="icon-file-text2"></i> View Details</a></li>
                                                        </ul>
                                                    </li>
                                                </ul>
                                            </td>
                                        </tr>

                                    </tbody>
                                </table>
                            </div>
                            <!-- /task manager table -->
                            <!-- SUMMARY -->
                            <div class="row" ng-show="detailTag == 1">
                                <div class="col-sm-6 col-md-3">
                                    <div class="panel panel-body bg-blue-800 has-bg-image">
