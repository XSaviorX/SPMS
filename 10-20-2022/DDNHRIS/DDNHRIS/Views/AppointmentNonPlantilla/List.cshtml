                </div>
                <div class="modal-footer">                 
                    <button type="button" class="btn btn-secondary " data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-primary " ng-disabled="bDisable" ng-click="addAppointee(empData)"> <i class="icon-check2"></i> Add Appointee</button>
                </div>
            </div>
        </div>
    </div>
    
    <!-- Primary modal -->
    <div id="modalPosting" class="modal fade" data-keyboard="false">
        <div class="modal-dialog modal-sm">
            <div class="modal-content">
                <div class="modal-header bg-primary">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h6 class="modal-title"> <i class="icon-stamp"></i> &nbsp; {{postData.fullNameFirst}} <br /> <small class="text-white"> &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; {{postData.positionTitle}}</small></h6>
                </div>
                <div class="modal-body">

                    <div class="form-group">
                        <div class="row">

                            <div class="col-md-12">
                                <label class="no-margin-bottom">Assumption Date <span class="text-danger">*</span>  </label>
                                <input type="date" class="form-control text-semibold" ng-model="postData.periodFrom">
                            </div>

                        </div>
                    </div>

                    <div class="row">

                        <div class="col-md-12">
                            <label class="no-margin-bottom">Work Group <span class="text-danger">*</span></label>
                            <select class="form-control select2 select-Custom" ng-model="workGroupCode" style="width:100%;"
                                    ng-options="c.workGroupCode as c.workGroupName for c in workGroupList" value="{{c.workGroupCode}}"
                                    id="workGroup">
                                <option value="" selected="">-- Select --</option>
                            </select>
                        </div>

                    </div>

                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-link" data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-primary" ng-disabled="bDisable" ng-click="postAppointmentItem(postData)">Confirm</button>
                </div>
            </div>
        </div>
    </div>

    <!-- Primary modal -->
    <div id="modalSearchAppointee" class="modal fade" data-keyboard="false">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header bg-primary">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h6 class="modal-title"> Search </h6>
                </div>
                <div class="modal-body">

                    <div class="form-group">
                        <div class="row">

                            <div class="col-md-12">
                                <div class="form-group form-group-material">

                                    <div class="input-group">
                                        <input type="text" class="form-control" placeholder="Search here..." ng-model="nameSearch">
                                        <span class="input-group-addon cursor-pointer" ng-disabled="bDisable" ng-click="searchAppointeeByName(nameSearch)"><i class="icon-search4"></i> </span>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>


                    <div class="form-group">
                        <div class="row">

                            <div class="col-md-12">
                                <div class="form-group form-group-material">
                                    <table class="table table-bordered table-striped table-sm">
                                        @*<thead>
                                                <tr>
                                                    <th class="text-center" style="width:40px!important;">#</th>
                                                    <th>Name of Appointee</th>

                                                </tr>
                                            </thead>*@
                                        <tbody>
                                            <tr ng-repeat="item in searchApptList">
                                                <td>
                                                    {{$index+1}}
                                                </td>
                                                <td>
                                                    <span class="text-semibold text-blue-800">
                                                        {{item.fullNameLast}}
                                                    </span>
                                                    <br />
                                                    <small>
                                                        {{item.projectName}}
                                                    </small>
                                                </td>
                                                <td>
                                                    {{item.appointmentName}}
                                                </td>

                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>

                        </div>
                    </div>


                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-link" data-dismiss="modal">Close</button>

                </div>
            </div>
        </div>
    </div>

</div>
