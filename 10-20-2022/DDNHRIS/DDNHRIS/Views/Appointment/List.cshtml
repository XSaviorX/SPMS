@{
    ViewBag.Title = "List";
}

<link href="~/Content/assets/plugins/select2/select2.min.css" rel="stylesheet" />
<link href="~/Content/assets/plugins/toastr/toastr.min.css" rel="stylesheet" />
<style>
    .page-title {
        padding: 20px 36px 10px 0;
        display: block;
        position: relative;
    }

    .tbl-sm > tbody > tr > td, .tbl-sm > tbody > tr > th, .tbl-sm > thead > tr > td {
        padding: 7px 20px;
        line-height: 1.2;
        vertical-align: top;
    }
</style>

<div ng-controller="AppointmentList">
    <!-- Page header -->
    <div class="page-header">
        <div class="page-header-content">
            <div class="page-title">
                <h4><i class="icon-books"></i> <span class="text-semibold">APPOINTMENT</span> Plantilla</h4>
            </div>
            <div class="heading-elements">

            </div>
        </div>

        <!-- Toolbar -->
        <div class="navbar navbar-default navbar-component navbar-xs">
            <ul class="nav navbar-nav visible-xs-block">
                <li class="full-width text-center"><a data-toggle="collapse" data-target="#navbar-filter"><i class="icon-menu7"></i></a></li>
            </ul>
            <div class="navbar-collapse collapse" id="navbar-filter">
                <ul class="nav navbar-nav">
                    <li class="active"><a href="#pending" data-toggle="tab"><i class="icon-stack-text position-left"></i> Pending Appointment</a></li>
                    <li><a href="#posted" data-toggle="tab"><i class="icon-file-check position-left"></i> Posted Appointment</a></li>
                    <li ng-click="showRAIAppt()"><a href="#rai" data-toggle="tab"><i class="icon-file-check position-left"></i> CSC RAI</a></li>
                    <li><a href="#incoming" data-toggle="tab"><span ng-show="incomingCount >= 1" class="badge badge-danger badge-inline position-right">{{incomingCount}}</span>&nbsp;<i class="icon-folder-upload3 position-left"></i>Incoming </a></li>
                    <li><a href="#tempo" data-toggle="tab"><span ng-show="tempCounter >= 1" class="badge badge-danger badge-inline position-right">{{tempCounter}}</span>&nbsp;<i class="icon-folder6 position-left"></i>Temporary </a></li>
                </ul>
                <div class="navbar-right">
                    <ul class="nav navbar-nav">
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="icon-gear"></i> <span class="visible-xs-inline-block position-right"> Options</span> <span class="caret"></span></a>
                            <ul class="dropdown-menu dropdown-menu-right">
                                <li><a href="~/Appointment/AppointeePosting"><i class="icon-user-check"></i> Co-Term Posting </a></li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <!-- /toolbar -->
    </div>

    <!-- Content area -->
    <div class="content">
        <!-- User profile -->
        <div class="row">

            <div class="col-lg-12">
                <div class="tabbable">
                    <div class="tab-content">

                        <!-- APPOINTMENT -->
                        <div class="tab-pane fade in active" id="pending">
                            <!-- Profile info -->
                            <div class="panel panel-flat">
                                <div class="panel-heading">
                                    <h5 class="panel-title"> Pending Appointment</h5>
                                </div>
                                <div class="panel-body">
                                    <table class="table">
                                        <tr ng-repeat="item in  appPending">
                                            <td>
                                                {{$index+1}}
                                            </td>
                                            <td>
                                                {{item.fullName}}
                                            </td>
                                            <td>{{item.positionTitle}}</td>
                                            <td class="text-center text-blue-800 text-bold">
                                                <ul class="icons-list">
                                                    <li class="dropdown">
                                                        <a href="#" class="dropdown-toggle" data-toggle="dropdown"> Forms <i class="icon-arrow-down12"></i> </a>
                                                        <ul class="dropdown-menu dropdown-menu-right">
                                                            <li ng-click="printApptTelerik('APPT', item.appointmentCode)"> <a href="#"><i class="icon-file-text2"></i> Appointment</a></li>
                                                            <li ng-click="printApptTelerik('PDF', item.appointmentCode)"> <a href="#"><i class="icon-file-text2"></i> PDF</a></li>
                                                            <li ng-click="printApptTelerik('PDFBACK', item.appointmentCode)"> <a href="#"><i class="icon-file-text2"></i> PDF Back</a></li>
                                                            <li ng-click="CheckCSCLimit(item)"> <a href="#"><i class="icon-file-text2"></i> CSC Limitation</a></li>
                                                            <li ng-click="CheckAssumption(item)"> <a href="#"><i class="icon-file-text2"></i> Assumption</a></li>
                                                            <li ng-click="CheckOathOffice(item)"> <a href="#"><i class="icon-file-text2"></i> Oath of Office</a></li>
                                                            <li ng-click="PrintCrystalReport('FUNDS', item.appointmentCode)"> <a href="#"><i class="icon-file-text2"></i> Funds Availability</a></li>
                                                            <li ng-click="printApptTelerik('RAI', item.appointmentCode)"> <a href="#"><i class="icon-file-text2"></i> RAI</a></li>
                                                        </ul>
                                                    </li>

                                                    <li class="dropdown" style="margin-left:15px;">
                                                        <a href="#" class="dropdown-toggle" data-toggle="dropdown"> Option <i class="icon-arrow-down12"></i> </a>
                                                        <ul class="dropdown-menu dropdown-menu-right">
                                                            <li ng-click="viewAppointmentData(item)"><a href="#"><i class="icon-file-check"></i> View Data</a></li>
                                                            <li ng-click="postingModal(item)"><a href="#"><i class="icon-checkmark4"></i> Posting</a></li>
                                                            <li><a href="#"><i class="icon-cross2 text-danger"></i> Cancel Appointment</a></li>
                                                        </ul>
                                                    </li>
                                                </ul>
                                            </td>

                                        </tr>
                                    </table>
                                </div>
                            </div>

                        </div>

                        <!-- POSTED -->
                        <div class="tab-pane fade" id="posted">
                            <!-- Profile info -->
                            <div class="panel panel-flat">
                                <div class="panel-heading">
                                    <h5 class="panel-title"> Posted Appointment</h5>
                                </div>
                                <div class="panel-body">
                                    <table class="table">
                                        <tr ng-repeat="item in  appPosted">
                                            <td>
                                                {{$index+1}}
                                            </td>
                                            <td>
                                                {{item.fullName}}
                                            </td>
                                            <td>{{item.positionTitle}}</td>
                                            <td class="text-center text-blue-800 text-bold">
                                                <ul class="icons-list">
                                                    <li class="dropdown">
                                                        <a href="#" class="dropdown-toggle" data-toggle="dropdown"> Forms <i class="icon-arrow-down12"></i> </a>
                                                        <ul class="dropdown-menu dropdown-menu-right">
                                                            <li ng-click="printApptTelerik('APPT', item.appointmentCode)"> <a href="#"><i class="icon-file-text2"></i> Appointment</a></li>
                                                            <li ng-click="printApptTelerik('PDF', item.appointmentCode)"> <a href="#"><i class="icon-file-text2"></i> PDF</a></li>
                                                            <li ng-click="printApptTelerik('PDFBACK', item.appointmentCode)"> <a href="#"><i class="icon-file-text2"></i> PDF Back</a></li>
                                                            <li ng-click="CheckCSCLimit(item)"> <a href="#"><i class="icon-file-text2"></i> CSC Limitation</a></li>
                                                            <li ng-click="CheckAssumption(item)"> <a href="#"><i class="icon-file-text2"></i> Assumption</a></li>
                                                            <li ng-click="CheckOathOffice(item)"> <a href="#"><i class="icon-file-text2"></i> Oath of Office</a></li>
                                                            <li ng-click="PrintCrystalReport('FUNDS', item.appointmentCode)"> <a href="#"><i class="icon-file-text2"></i> Funds Availability</a></li>
                                                            <li ng-click="printApptTelerik('RAI', item.appointmentCode)"> <a href="#"><i class="icon-file-text2"></i> RAI</a></li>
                                                        </ul>
                                                    </li>

                                                    <li class="dropdown" style="margin-left:15px;">
                                                        <a href="#" class="dropdown-toggle" data-toggle="dropdown"> Option <i class="icon-arrow-down12"></i> </a>
                                                        <ul class="dropdown-menu dropdown-menu-right">
                                                            <li ng-click="viewAppointmentData(item)"><a href="#"><i class="icon-file-check"></i> View Data</a></li>
                                                            <li ng-click="postingModal(item)"><a href="#"><i class="icon-checkmark4"></i> Posting</a></li>
                                                            <li><a href="#"><i class="icon-cross2 text-danger"></i> Cancel Appointment</a></li>
                                                        </ul>
                                                    </li>
                                                </ul>
                                            </td>

                                        </tr>
                                    </table>
                                </div>
                            </div>

                        </div>

                        <!-- RAI  -->
                        <div class="tab-pane fade" id="rai">

                            <!-- RAI LIST -->
                            <div class="panel panel-flat" ng-show="raiTab == 0">
                                <div class="panel-heading">
                                    <h5 class="panel-title">
                                        Report on Appointment Issued (RAI)
                                        <span class="btn  btn-link text-semibold text-blue-800 pull-right" ng-click="modalRAI()">
                                            <i class="icon-stack-plus"></i> Create RAI
                                        </span>
                                    </h5>
                                </div>
                                <div class="panel-body">
                                    <table class="table table-striped table-hover ">
                                        <thead>
                                            <tr>
                                                <th class="text-center" style="width:50px">#</th>
                                                <th style="width:50%;">RAI Name</th>

                                                <th class="text-center">Date/Time</th>
                                                <th style="width:50px"></th>
                                            </tr>
                                        </thead>
                                        <tr ng-repeat="item in  RAIAppList">
                                            <td class="text-center">
                                                {{$index+1}}
                                            </td>
                                            <td>
                                                <span class="text-semibold text-blue-800 cursor-pointer" ng-click="viewRAIData(item)">{{item.RAIName}} </span>
                                            </td>

                                            <td class="text-center">
                                                {{formatDate(item.transDT)}}
                                            </td>
                                            <td class="text-center text-blue-800 text-bold">
                                                <ul class="icons-list">

                                                    <li class="dropdown" style="margin-left:15px;">
                                                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">  <i class="icon-arrow-down12"></i> </a>
                                                        <ul class="dropdown-menu dropdown-menu-right">
                                                            <li ng-click="viewRAIData(item)"><a href="#"><i class="icon-file-check"></i> View Data</a></li>
                                                            <li ng-click="removeRAI(item)"><a href="#"><i class="icon-cross2 text-danger"></i> Cancel RAI </a></li>
                                                        </ul>
                                                    </li>
                                                </ul>
                                            </td>

                                        </tr>
                                    </table>
                                </div>
                            </div>

                            <!-- RAI APPOINTMENT LIST -->
                            <div class="panel panel-flat" ng-show="raiTab == 1">
                                <div class="panel-heading">
                                    <h5 class="panel-title text-blue-800">
                                        <i class="icon-arrow-left52 text-blue-800 text-semibold cursor-pointer" ng-click="backToRAIList()"></i> {{raiData.RAIName}}

                                    </h5>

                                    <div class="heading-elements">
                                        <ul class="icons-list">
                                            <li class="text-blue-800 cursor-pointer text-semibold" ng-click="modalOpenRAIAPPPTAdd()"><span><i class="icon-stack-plus"></i> Select Appointment </span> &nbsp;&nbsp;</li>
                                            @*<li class="text-blue-800 cursor-pointer text-semibold" ng-click="printRAI()"> <span> <i class="icon-printer2 position-left"></i></span>Print Report </li>*@
                                            <li class="dropdown" style="margin-left:15px;">
                                                <a href="#" class="dropdown-toggle text-blue-800 cursor-pointer" data-toggle="dropdown"><span> <i class="icon-printer2 position-left"></i></span>Preview <i class="icon-arrow-down12"></i> </a>
                                                <ul class="dropdown-menu dropdown-menu-right">
                                                    <li ng-click="printRAI(0)"><a href="#"><i class="icon-file-check"></i> Print RAI</a></li>
                                                    <li ng-click="printRAIBACK()"><a href="#"><i class="icon-file-check"></i> Print RAI (Back) </a></li>
                                                    <li ng-click="printRAI(1)"><a href="#"><i class="icon-file-check"></i> Print RAI (Excel)</a></li>
                                                   
                                                </ul>
                                            </li>
                                        </ul>
                                    </div>

                                </div>
                                <div class="panel-body">


                                    <div class="row">
                                        <div class="col-lg-12">
                                            <table class="table table-bordered table-striped table-hover ">
                                                <thead>
                                                    <tr>
                                                        <th class="text-center" style="width:50px">#</th>
                                                        <th class="text-center">DATE</th>
                                                        <th class="text-center">NAME OF APPOINTEE</th>
                                                        <th class="text-center">POSITION TITLE</th>
                                                        <th class="text-center">NATURE OF APPT</th>
                                                        <th class="text-center">PUBLICATION</th>
                                                        <th style="width:50px"></th>
                                                    </tr>
                                                </thead>
                                                <tr ng-repeat="item in raiAppList">
                                                    <td class="text-center">
                                                        {{$index+1}}
                                                    </td>

                                                    <td>{{formatDate(item.dateIssued)}}</td>
                                                    <td>
                                                        {{item.lastName}}, {{item.firstName}} {{item.extnName}} {{item.middleName}}
                                                    </td>
                                                    <td>{{item.positionTitle}}<span class="text-grey">({{item.salaryGrade}})</span></td>

                                                    <td>{{item.natureOfAppointment}}  </td>
                                                    <td class="text-center">
                                                        {{formatDate(item.CSCPostedDate)}} - {{formatDate(item.CSCClosingDate)}}
                                                    </td>
                                                    <td class="text-center text-blue-800 text-bold">
                                                        <ul class="icons-list">

                                                            <li class="dropdown" style="margin-left:15px;">
                                                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">  <i class="icon-arrow-down12"></i> </a>
                                                                <ul class="dropdown-menu dropdown-menu-right">
                                                                    <li ng-click="removeRAIItem(item)"><a href="#"><i class="icon-cross2 text-danger"></i> Remove </a></li>
                                                                </ul>
                                                            </li>
                                                        </ul>
                                                    </td>

                                                </tr>

                                                <tfoot ng-if="raiAppList.length == 0 ">
                                                    <tr>
                                                        <td colspan="7" style="padding:0px; margin-bottom:0px;">

                                                            <div class="alert alert-danger  no-border" ng-if="raiAppList.length == 0 " style="margin-bottom:0px;">

                                                                <h5>
                                                                    NO APPOINTMENT FOUND!
                                                                    <span class="btn btn-danger btn-xs pull-right" ng-click="removeRAI(raiData)">
                                                                        <i class="icon-trash-alt"></i> Delete
                                                                    </span>

                                                                </h5>

                                                            </div>


                                                        </td>
                                                    </tr>
                                                </tfoot>

                                            </table>

                                        </div>
                                    </div>



                                    <br />

                                    <div class="row" ng-if="raiAppList.length > 0 ">
                                        <div class="col-lg-12">
                                            <table class="table table-bordered tbl-sm">
                                                <thead>
                                                    <tr>
                                                        <th class="text-center" colspan="2"><strong>CHECK LIST OF COMMON REQUIREMENTS</strong></th>
                                                        <th class="text-center" style="width:20%">HRMO</th>
                                                        <th class="text-center" style="width:20%">CSC FO</th>
                                                    </tr>
                                                </thead>

                                                <tbody style="font-size:.9em;">

                                                    <tr>
                                                        <td class="text-center" style="width:50px;">1</td>
                                                        <td>
                                                            <strong>APPOINTMENT FORMS</strong>
                                                            <p style="margin:0px;">
                                                                <small>
                                                                    (CS Form No. 33-B, Revised 2018) -  Original CSC Copy Appointment {{raiData.chkList_ApptForms}}
                                                                </small>
                                                            </p>
                                                        </td>
                                                        <td class="text-center text-size-large">
                                                            <button class="btn btn-default btn-xs" ng-click="updateCheckList(1)">
                                                                <span class="text-semibold text-blue-800" ng-show="raiData.chkList_ApptForms == 1">
                                                                    <i class="icon-check"></i>
                                                                </span>
                                                                <span ng-show="raiData.chkList_ApptForms != 1">
                                                                    <strong>N/A</strong>
                                                                </span>
                                                            </button>
                                                        </td>
                                                        <td></td>
                                                    </tr>
                                                    <tr>
                                                        <td class="text-center">2</td>
                                                        <td>
                                                            <strong>PLANTILLA OF CASUAL APPOINTMENT</strong>
                                                            <p style="margin:0px;">
                                                                <small>
                                                                    CSC FORM No. 34-B, D, E or F) -  Original Copy
                                                                </small>
                                                            </p>
                                                        </td>
                                                        <td class="text-center text-size-large">
                                                            <button class="btn btn-default btn-xs" ng-click="updateCheckList(2)">
                                                                <span class="text-semibold text-blue-800" ng-show="raiData.chkList_Casual == 1">
                                                                    <i class="icon-check"></i>
                                                                </span>
                                                                <span ng-show="raiData.chkList_Casual != 1">
                                                                    <strong>N/A</strong>
                                                                </span>
                                                            </button>
                                                        </td>
                                                        <td></td>
                                                    </tr>
                                                    <tr>
                                                        <td class="text-center">3</td>
                                                        <td>
                                                            <strong>PERSONAL DATA SHEET</strong>
                                                            <p style="margin:0px;">
                                                                <small>
                                                                    CSC FORM No. 212, Revised 2017
                                                                </small>
                                                            </p>
                                                        </td>
                                                        <td class="text-center text-size-large">
                                                            <button class="btn btn-default btn-xs" ng-click="updateCheckList(3)">
                                                                <span class="text-semibold text-blue-800" ng-show="raiData.chkList_PDS == 1">
                                                                    <i class="icon-check"></i>
                                                                </span>
                                                                <span ng-show="raiData.chkList_PDS != 1">
                                                                    <strong>N/A</strong>
                                                                </span>
                                                            </button>
                                                        </td>
                                                        <td></td>
                                                    </tr>

                                                    <tr>
                                                        <td class="text-center">4</td>
                                                        <td>
                                                            <strong>ORIGINAL COPY OF AUTHENTICATED CERTIFICATE OF ELIGIBILITY/ RATING/ LICENSE </strong>
                                                            <p style="margin:0px;">
                                                                <small>
                                                                    Except if the eligibility has been previously authenticated in 2004 or onward and recorded by the CSC <br />
                                                                    CSC FORM No. 212, Revised 2017
                                                                </small>
                                                            </p>
                                                        </td>
                                                        <td class="text-center text-size-large">
                                                            <button class="btn btn-default btn-xs" ng-click="updateCheckList(4)">
                                                                <span class="text-semibold text-blue-800" ng-show="raiData.chkList_Eligibility == 1">
                                                                    <i class="icon-check"></i>
                                                                </span>
                                                                <span ng-show="raiData.chkList_Eligibility != 1">
                                                                    <strong>N/A</strong>
                                                                </span>
                                                            </button>
                                                        </td>
                                                        <td></td>
                                                    </tr>


                                                    <tr>
                                                        <td class="text-center">5</td>
                                                        <td>
                                                            <strong>POSITION DESCRIPTION FORM</strong>
                                                            <p style="margin:0px;">
                                                                <small> DBM-CSC Form No. 1, Revised 2018</small>
                                                            </p>
                                                        </td>
                                                        <td class="text-center text-size-large">
                                                            <button class="btn btn-default btn-xs" ng-click="updateCheckList(5)">
                                                                <span class="text-semibold text-blue-800" ng-show="raiData.chkList_PDF == 1">
                                                                    <i class="icon-check"></i>
                                                                </span>
                                                                <span ng-show="raiData.chkList_PDF != 1">
                                                                    <strong>N/A</strong>
                                                                </span>
                                                            </button>
                                                        </td>
                                                        <td></td>
                                                    </tr>

                                                    <tr>
                                                        <td class="text-center">6</td>
                                                        <td>
                                                            <strong>OATH OF OFFICE</strong>
                                                            <p style="margin:0px;">
                                                                <small> (CSC Form No. 32, Revised 2018)</small>
                                                            </p>
                                                        </td>
                                                        <td class="text-center text-size-large">

                                                            <button class="btn btn-default btn-xs" ng-click="updateCheckList(6)">
                                                                <span class="text-semibold text-blue-800" ng-show="raiData.chkList_Oath == 1">
                                                                    <i class="icon-check"></i>
                                                                </span>
                                                                <span ng-show="raiData.chkList_Oath != 1">
                                                                    <strong>N/A</strong>
                                                                </span>
                                                            </button>

                                                        </td>
                                                        <td></td>
                                                    </tr>
                                                    <tr>
                                                        <td class="text-center">7</td>
                                                        <td>
                                                            <strong>CERTIFICATE OF ASSUMPTION TO DUTY</strong>
                                                            <p style="margin:0px;">
                                                                <small>  (CSC Form No. 4, Revised 2018)</small>
                                                            </p>
                                                        </td>
                                                        <td class="text-center text-size-large">
                                                            <button class="btn btn-default btn-xs" ng-click="updateCheckList(7)">
                                                                <span class="text-semibold text-blue-800" ng-show="raiData.chkList_Assumption == 1">
                                                                    <i class="icon-check"></i>
                                                                </span>
                                                                <span ng-show="raiData.chkList_Assumption != 1">
                                                                    <strong>N/A</strong>
                                                                </span>
                                                            </button>
                                                        </td>
                                                        <td></td>
                                                    </tr>
                                                </tbody>

                                            </table>
                                        </div>



                                    </div>



                                </div>
                            </div>

                        </div>

                        <!-- INCOMING APPOINTMENT -->
                        <div class="tab-pane fade" id="incoming">

                            <div class="panel panel-flat">
                                <div class="panel-heading">
                                    <h5 class="panel-title"> Incoming Appointment</h5>

                                </div>
                                <div class="panel-body">
                                    <table class="table table-striped table-hover table-bordered">
                                        <tr ng-repeat="item in appIncoming">
                                            <td>
                                                {{$index+1}}
                                            </td>
                                            <td>
                                                <span class="text-blue-700 text-semibold" style="cursor:pointer">
                                                    {{item.applicantName}}
                                                </span>
                                            </td>
                                            <td>
                                                {{item.positionTitle}}
                                            </td>
                                            <td>
                                                {{item.departmentName}}
                                            </td>
                                            <td class="text-center" style="width:10px; ">

                                                <button class="btn btn-primary btn-xs text-semibold no-margin-top no-margin-bottom" ng-disabled="bDisable" ng-click="createAppointment(item)">
                                                    Create Appointment
                                                </button>

                                            </td>

                                        </tr>
                                    </table>

                                    <div class="alert alert-danger  no-border" ng-if="incomingCount == 0">

                                        <h5>
                                            NO INCOMING APPOINTMENT!
                                        </h5>


                                    </div>



                                </div>
                            </div>

                        </div>


                        <!-- INCOMING APPOINTMENT -->
                        <div class="tab-pane fade" id="tempo">

                            <div class="panel panel-flat">
                                <div class="panel-heading">
                                    <h5 class="panel-title"> Temporary Appointment</h5>
                                </div>
                                <div class="panel-body">
                                    <table class="table table-striped table-hover table-bordered">
                                        <tr ng-repeat="item in temporary">
                                            <td class="text-center" style="width:50px!important">
                                                {{$index+1}}
                                            </td>
                                            <td>
                                                <span class="text-blue-700 text-semibold" style="cursor:pointer">
                                                    {{item.fullNameLast}}
                                                </span>
                                            </td>
                                            <td>
                                                {{item.positionTitle}} <span class="text-muted">({{item.salaryGrade}})</span>
                                            </td>

                                            <td>
                                                {{item.shortDepartmentName}}
                                            </td>
                                            <td class="text-center" style="width:120px; ">
