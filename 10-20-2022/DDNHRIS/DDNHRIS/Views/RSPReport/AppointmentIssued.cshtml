@{
    ViewBag.Title = "RAI";
}

<link href="~/Content/assets/plugins/select2/select2.min.css" rel="stylesheet" />
<link href="~/Content/assets/plugins/toastr/toastr.min.css" rel="stylesheet" />
<style>
    .page-title {
        padding: 20px 36px 10px 0;
        display: block;
        position: relative;
    }

    .tbl-sm > tbody > tr > td, .tbl-sm > tbody > tr > th, .tbl-sm > thead > tr > td {
        padding: 3px 20px;
        line-height: 1.2;
        vertical-align: top;
    }
</style>

<div ng-controller="RAI">
    <!-- Page header -->
    <div class="page-header">
        <div class="page-header-content">
            <div class="page-title">
                <h4><i class="icon-books"></i> <span class="text-semibold">REPORT ON APPOINTMENT ISSUED</span> </h4>
            </div>
            <div class="heading-elements">

            </div>
        </div>

        <!-- Toolbar -->
        <div class="navbar navbar-default navbar-component navbar-xs">
            <ul class="nav navbar-nav visible-xs-block">
                <li class="full-width text-center"><a data-toggle="collapse" data-target="#navbar-filter"><i class="icon-menu7"></i></a></li>
            </ul>
            <div class="navbar-collapse collapse" id="navbar-filter">
                <ul class="nav navbar-nav">
                    <li class="active"><a href="#appointment" data-toggle="tab"><i class="icon-stack-text position-left"></i> Appointment Issued</a></li>
                    <li ng-click="showRAIList()"><a href="#railist" data-toggle="tab"><i class="icon-file-check position-left"></i> RAI List</a></li>
                </ul>
                <div class="navbar-right">
                    <ul class="nav navbar-nav">

                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="icon-gear"></i> <span class="visible-xs-inline-block position-right"> Options</span> <span class="caret"></span></a>
                            <ul class="dropdown-menu dropdown-menu-right">
                                <li><a href="#" ng-click="generateRAI()"><i class="icon-make-group"></i>Generate RAI</a></li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <!-- /toolbar -->

    </div>



    <!-- Content area -->
    <div class="content">
        <!-- User profile -->
        <div class="row">

            <div class="col-lg-12">
                <div class="tabbable">
                    <div class="tab-content">

                        <!-- APPOINTMENT ISSUED (PENDING) -->


                        <div class="tab-pane panel panel-flat active" id="appointment">
                            <div class="panel-heading">
                                <h5 class="panel-title no-margin-top no-margin-bottom  text-blue-800 text-semibold">
                                    APPOINTMENT ISSUED
                                </h5>

                                <div class="heading-elements">
                                    <input class="form-control" type="text" placeholder="Type to Search" style="max-width:550px;" ng-model='searchString' />
                                </div>
                            </div>
                            <div class="table-responsive">
                                <table class="table table-striped table-hover table-bordered" style="border-bottom:2px solid #f1f1f1;">
                                    <thead>
                                        <tr>
                                            <th>#</th>
                                            <th>APPOINTMENT NAME #</th>
                                            <th>STATUS</th>
                                            <th>FUND SOURCE</th>
                                            <th class="text-center">
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody>

                                        <tr ng-repeat="item in appntList | filter :searchString">
                                            <td>
                                                {{$index+1}}
                                            </td>

                                            <td class="cursor-pointer" ng-click="viewApptDetails(item)">
                                                <span class="text-blue-800 text-bold">{{item.appointmentName}}</span>
                                            </td>
                                            <td class="text-center">{{item.employmentStatus}}</td>
                                            <td>
                                                {{item.projectName}}
                                            </td>

                                            <td class="text-center" style="min-width:90px!important;">
                                                <ul class="icons-list">

                                                    <li class="divider"></li>
                                                    <li class="dropdown">
                                                        <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="icon-cog3"></i><i class="icon-arrow-down12 text-muted"></i></a>
                                                        <ul class="dropdown-menu dropdown-menu-right">
                                                            <li><a href="#" ng-click="viewApptDetails(item)"><i class="icon-database-edit2"></i> Generate RAI </a></li>

                                                        </ul>
                                                    </li>

                                                </ul>
                                            </td>
                                        </tr>


                                    </tbody>
                                </table>

                                <br />
                                <br />

                            </div>
                        </div>
                        <!-- /basic table -->
                        <!-- GENERATED RAI -->
                        <div class="tab-pane panel panel-flat" id="railist">
                            <div class="panel-heading">
                                <h5 class="panel-title no-margin-top no-margin-bottom  text-blue-800 text-semibold">
                                    REPORT ON APPOINMENT ISSUED
                                </h5>

                                <div class="heading-elements">
                                    <input class="form-control" type="text" placeholder="Type to Search" style="max-width:550px;" ng-model='searchString' />
                                </div>
                            </div>
                            <div class="table-responsive">
                                <table class="table table-striped table-hover table-bordered">
                                    <thead>
                                        <tr>
                                            <th>#</th>
                                            <th>REPORT CODE</th>
                                            <th class="text-center">REPORT DATE</th>
                                            <th class="text-center">ITEM COUNT</th>
                                            <th class="text-center">Date/Time</th>
                                            <th class="text-center">
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody>

                                        <tr ng-repeat="item in raiList | filter :searchString">
                                            <td>
                                                {{$index+1}}
                                            </td>

                                            <td class="cursor-pointer" ng-click="printRAI(item.RAICode)">
                                                <span class="text-blue-800 text-semibold">  {{item.appointmentCode}} </span>
                                            </td>
                                            <td>
                                                {{item.appointmentName}}
                                            </td>
                                            <td class="text-center">{{item.projectName}}</td>

                                            <td class="text-center"></td>
                                            <td></td>
                                            <td class="text-center" style="min-width:90px!important;">
                                                <ul class="icons-list">

                                                    <li class="divider"></li>
                                                    <li class="dropdown">
                                                        <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="icon-cog3"></i><i class="icon-arrow-down12 text-muted"></i></a>
                                                        <ul class="dropdown-menu dropdown-menu-right">
                                                            <li><a href="#" ng-click="viewApptDetails(item)"><i class="icon-database-edit2"></i> View Details </a></li>
                                                            <li ng-show="item.tag == 0"><a href="#" ng-click="markAsFinal(item)"><i class="icon-file-check"></i> Forward to PACCO </a></li>
                                                            <li ng-if="item.tag == 0"><a href="#" ng-click="deleteAppointment(item)"><i class="icon-cross2 text-danger"></i> Delete</a></li>
                                                        </ul>
                                                    </li>

                                                </ul>
                                            </td>
                                        </tr>


                                    </tbody>
                                </table>
                            </div>
                        </div>
                        
                    </div>
                </div>
            </div>


        </div>

    </div>


    <!-- Modal APPOINTMENT POSTING-->
    <div class="modal fade" id="modalAppointmentPosting" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="form-section" style="margin-top:0px; padding-top:0px;">
                        Appointment Posting
                    </h5>
                </div>
                <div class="modal-body">

                    <div class="form-group">
                        <div class="row">

                            <div class="col-md-3">
                                <label class="text-muted no-margin-bottom">Item No</label>
                                <input type="text" ng-readonly="true" ng-model="appointmentData.itemNo" class="form-control text-semibold">
                            </div>

                            <div class="col-md-7">
                                <label class="text-muted no-margin-bottom"> Name Prefix <span class="text-danger">*</span></label>
                                <input type="text" ng-readonly="true" ng-model="appointmentData.positionTitle" class="form-control text-semibold" placeholder="MS./MR./DR./ATTY.">
                            </div>

                            <div class="col-md-2">
                                <label class="text-muted no-margin-bottom"> SG/Step <span class="text-danger">*</span></label>
                                <label class="form-control text-semibold" ng-readonly="true">
                                    {{appointmentData.salaryGrade}}/{{appointmentData.step}}
                                </label>
                            </div>

                        </div>
                    </div>

                    <div class="form-group">
                        <div class="row">

                            <div class="col-md-8">
                                <label class="text-muted no-margin-bottom">Appointee Name <span class="text-danger">*</span> </label>
                                <input id="text" ng-readonly="true" type="text" ng-model="appointmentData.fullNameTitle" class="form-control text-semibold">
                            </div>

                            <div class="col-md-4">
                                <label class="text-muted no-margin-bottom">Assumption Date <span class="text-danger">*</span> </label>
                                <input class="form-control required text-semibold" type="date" ng-model="appointmentData.effectivityDate" name="date">
                            </div>

                        </div>
                    </div>




                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary " ng-disabled="bDisable" data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-primary " ng-disabled="bDisable" ng-click="postingAppointment()">Post Appointment</button>
                </div>
            </div>
        </div>

    </div>

    <!-- Modal APPOINTMENT CREATE-->
    <div class="modal fade" id="modalAppointment" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="form-section" style="margin-top:0px; padding-top:0px;"><i class="icon-clipboard3"></i>  <span class="text-bold text-blue-700"> {{appointmentData.fullName}}  </span> </h5>
                </div>
                <div class="modal-body">

                    <div class="form-group">
                        <div class="row">

                            <div class="col-md-6">
                                <label class="text-muted no-margin-bottom">Appointee Name (Title) <span class="text-danger">*</span> </label>
                                <input id="text" ng-readonly="true" type="text" ng-model="appointmentData.fullNameTitle" class="form-control text-semibold" placeholder="(DR. JUAN P. DELA CRUZ JR.)">
                            </div>

                            <div class="col-md-3">
                                <label class="text-muted no-margin-bottom"> Name Prefix <span class="text-danger">*</span></label>
                                <input type="text" ng-readonly="isReadOnly" ng-model="appointmentData.namePrefix" class="form-control text-semibold" placeholder="MS./MR./DR./ATTY.">
                            </div>

                            <div class="col-md-3">
                                <label class="text-muted no-margin-bottom"> Name Suffix</label>
                                <input type="text" ng-readonly="isReadOnly" ng-model="appointmentData.nameSuffix" class="form-control text-semibold" placeholder="CPA/MBA/MPA/PhD">
                            </div>

                        </div>
                    </div>

                    <div class="form-group">
                        <div class="row">

                            <div class="col-md-3">
                                <label class="text-muted no-margin-bottom">Item No</label>
                                <input type="text" ng-readonly="true" ng-model="appointmentData.itemNo" class="form-control text-semibold">
                            </div>

                            <div class="col-md-6">
                                <label class="text-muted no-margin-bottom"> Position Title</label>
                                <input type="text" ng-readonly="true" ng-model="appointmentData.positionTitle" class="form-control text-semibold">
                            </div>

                            <div class="col-md-2">
                                <label class="text-muted no-margin-bottom">Salary Grade </label>
                                <input type="text" ng-readonly="true" ng-model="appointmentData.salaryGrade" class="form-control text-semibold">
                            </div>

                            <div class="col-md-1">
                                <label class="text-muted no-margin-bottom">Step <span class="text-danger">*</span></label>

                                <select id="step" class="form-control select-Custom text-semibold" ng-disabled="isReadOnly" ng-model="appointmentData.step" style="width:100%;"
                                        ng-options="c.code as c.name for c in stepList" value="{{c.code}}"></select>
                            </div>

                        </div>
                    </div>

                    <div class="form-group">
                        <div class="row">

                            <div class="col-md-3">
                                <label class="text-muted no-margin-bottom">Nature o Apointment <span class="text-danger">*</span></label>
                                <select class="form-control select-Custom text-semibold" ng-model="appointmentData.appNatureCode" style="width:100%;"
                                        ng-options="c.appNatureCode as c.appNatureName for c in appNatureList" value="{{c.appNatureCode}}">
                                    <option value="" selected="">-- Select --</option>
                                </select>
                            </div>

                            <div class="col-md-3">
                                <label class="text-muted no-margin-bottom">Employment Status <span class="text-danger">*</span></label>
                                <select class="form-control select-Custom text-semibold" ng-model="appointmentData.employmentStatusCode" style="width:100%;"
                                        ng-options="c.employmentStatusCode as c.employmentStatus for c in employmentStatus" value="{{c.employmentStatusCode}}">
                                    <option value="" selected="">-- Select --</option>
                                </select>
                            </div>


                            <div class="col-md-6">
                                <label class="text-muted no-margin-bottom">Eligibility Code <span class="text-danger">*</span></label>
                                <select id="eligibility" class="form-control select-Custom text-semibold" ng-model="appointmentData.eligibilityCode" style="width:100%;"
                                        ng-options="c.eligibilityCode as c.eligibilityName for c in eligList" value="{{c.eligeligibilityCodeList}}">
                                    <option value="" selected="">-- Select --</option>
                                </select>
                            </div>

                        </div>
                    </div>

                    <div class="form-group">
                        <div class="row">

                            <div class="col-md-3">
                                <label class="text-muted no-margin-bottom">CSC Posted Date</label>
                                <input id="text" ng-readonly="true" type="text" ng-model="(formatDate(appointmentData.CSCPostedDate))" class="form-control text-semibold">
                            </div>
                            <div class="col-md-3">
                                <label class="text-muted no-margin-bottom"> CSC Closing Date</label>
                                <input type="text" ng-readonly="true" ng-model="(formatDate(appointmentData.CSCClosingDate))" class="form-control text-semibold">
                            </div>
                            <div class="col-md-3">
                                <label class="text-muted no-margin-bottom"> P.S.B. Date</label>
                                <input type="text" ng-readonly="true" ng-model="(formatDate(appointmentData.PSBDate))" class="form-control text-semibold">
                            </div>

                            <div class="col-md-2">
                                <label class="text-muted no-margin-bottom">Page #</label>
                                <input type="text" ng-readonly="true" ng-model=" appointmentData.pageNo" class="form-control text-semibold">
                            </div>

                        </div>
                    </div>


                    <div class="form-group">
                        <div class="row">

                            <div class="col-md-5">
                                <label class="text-muted no-margin-bottom"> Office Assignment <span class="text-danger">*</span></label>
                                <input type="text" ng-readonly="isReadOnly" ng-model="appointmentData.officeAssignment" class="form-control text-semibold">
                            </div>

                            <div class="col-md-4">
                                <label class="text-muted no-margin-bottom">Vice Of</label>
                                <input id="text" ng-readonly="isReadOnly" type="text" ng-model="appointmentData.viceOf" class="form-control text-semibold">
                            </div>
                            <div class="col-md-3">
                                <label class="text-muted no-margin-bottom"> Vice Of Reason</label>
                                <input type="text" ng-readonly="isReadOnly" ng-model="appointmentData.viceStatus" class="form-control text-semibold">
                            </div>



                        </div>
                    </div>


                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary " ng-disabled="bDisable" data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-primary " ng-disabled="bDisable" ng-click="submitAppointment(appointmentData)">Save Appointment</button>
                </div>
            </div>
        </div>

    </div>

    <!-- Modal APPOINTMENT EDIT -->
    <div class="modal fade" id="modalApptFormEdit" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-body">
                    <!-- Toolbar -->
                    <div class="navbar navbar-default navbar-component navbar-xs">
                        <ul class="nav navbar-nav visible-xs-block">
                            <li class="full-width text-center"><a data-toggle="collapse" data-target="#navbar-filter"><i class="icon-menu7"></i></a></li>
                        </ul>
                        <div class="navbar-collapse collapse" id="navbar-filter">

                            <ul class="nav navbar-nav  text-bold">
                                <li class="active"><a href="#appData" data-toggle="tab"><i class="icon-file-text2 position-left"></i> Appointment Data</a></li>
                                <li><a href="#appPDF" data-toggle="tab"><i class="icon-file-text2 position-left"></i> Other Forms Data </a></li>

                            </ul>

                            <div class="navbar-right">
                                <ul class="nav navbar-nav">
                                    <li class="dropdown">
                                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                            <i class="icon-printer2 position-left"></i>
                                            Report
                                            <span class="caret"></span>
                                        </a>
                                        <ul class="dropdown-menu dropdown-menu-right">
                                            <li ng-click="printApptReport('APPT', item.appointmentCode)"> <a href="#"><i class="icon-file-text2"></i> Appointment</a></li>
                                            <li ng-click="printApptReport('PDF', item.appointmentCode)"> <a href="#"><i class="icon-file-text2"></i> PDF</a></li>
                                            <li ng-click="printApptReport('PDFBACK', item.appointmentCode)"> <a href="#"><i class="icon-file-text2"></i> PDF Back</a></li>
                                            <li ng-click="printHRISReport('LIMITATION', item.appointmentCode)"> <a href="#"><i class="icon-file-text2"></i> CSC Limitation</a></li>
                                            <li ng-click="printHRISReport('ASSUMPTION', item.appointmentCode)"> <a href="#"><i class="icon-file-text2"></i> Assumption</a></li>
                                            <li ng-click="printHRISReport('OATH', item.appointmentCode)"> <a href="#"><i class="icon-file-text2"></i> Oath of Office</a></li>
                                            <li ng-click="printHRISReport('FUNDS', item.appointmentCode)"> <a href="#"><i class="icon-file-text2"></i> Funds Availability</a></li>
                                        </ul>
                                    </li>

                                </ul>
                            </div>

                        </div>
                    </div>
                    <!-- /toolbar -->

                    <div class="tabbable">
                        <div class="tab-content">

                            <!-- APPOINTMENT -->
                            <div class="tab-pane fade in active" id="appData">

                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-md-8">
                                            <h5 class="panel-title text-blue-800 text-bold"> APPOINTMENT DATA</h5>
                                        </div>

                                    </div>
                                </div>

                                <div class="form-group">
                                    <div class="row">

                                        <div class="col-md-6">
                                            <label class="text-muted no-margin-bottom">Appointee Title Name <span class="text-danger">*</span></label>
                                            <input id="text" ng-readonly="true" type="text" ng-model="formApp.fullName" class="form-control text-semibold" placeholder="(DR. JUAN P. DELA CRUZ JR.)">
                                        </div>

                                        <div class="col-md-3">
                                            <label class="text-muted no-margin-bottom"> Name Prefix <span class="text-danger">*</span></label>
                                            <input type="text" ng-readonly="isReadOnly" ng-model="formApp.namePrefix" class="form-control text-semibold" placeholder="MS./MR./DR./ATTY.">
                                        </div>

                                        <div class="col-md-3">
                                            <label class="text-muted no-margin-bottom"> Name Suffix</label>
                                            <input type="text" ng-readonly="isReadOnly" ng-model="formApp.nameSuffix" class="form-control text-semibold" placeholder="CPA/MBA/MPA/PhD">
                                        </div>

                                    </div>
                                </div>

                                <div class="form-group">
                                    <div class="row">

                                        <div class="col-md-3">
                                            <label class="text-muted no-margin-bottom">Item No</label>
                                            <input type="text" ng-readonly="true" ng-model="formApp.itemNo" class="form-control text-semibold">
                                        </div>

                                        <div class="col-md-6">
                                            <label class="text-muted no-margin-bottom"> Position Title</label>
                                            <input type="text" ng-readonly="true" ng-model="formApp.positionTitle" class="form-control text-semibold">
                                        </div>

                                        <div class="col-md-2">
                                            <label class="text-muted no-margin-bottom">Salary Grade</label>
                                            <input type="text" ng-readonly="true" ng-model="formApp.salaryGrade" class="form-control text-semibold">
                                        </div>

                                        <div class="col-md-1">
                                            <label class="text-muted no-margin-bottom">Step <span class="text-danger">*</span></label>
                                            <select id="step" class="form-control select-Custom text-semibold" ng-disabled="isReadOnly" ng-model="formApp.step" style="width:100%;"
                                                    ng-options="c.code as c.name for c in stepList" value="{{c.code}}">
                                                <option value="" selected="">-- Select --</option>
                                            </select>
                                        </div>

                                    </div>
                                </div>

                                <div class="form-group">
                                    <div class="row">

                                        <div class="col-md-3">
                                            <label class="text-muted no-margin-bottom">Nature o Apointment <span class="text-danger">*</span></label>
                                            <select class="form-control select-Custom text-semibold" ng-model="formApp.appNatureCode" ng-disabled="isReadOnly" style="width:100%;"
                                                    ng-options="c.appNatureCode as c.appNatureName for c in appNatureList" value="{{c.appNatureCode}}">
                                                <option value="" selected="">-- Select --</option>
                                            </select>
                                        </div>

                                        <div class="col-md-3">
                                            <label class="text-muted no-margin-bottom">Employment Status <span class="text-danger">*</span></label>
                                            <select class="form-control select-Custom text-semibold" ng-model="formApp.employmentStatusCode" ng-disabled="isReadOnly" style="width:100%;"
                                                    ng-options="c.employmentStatusCode as c.employmentStatus for c in employmentStatus" value="{{c.employmentStatusCode}}">
                                                <option value="" selected="">-- Select --</option>
                                            </select>
                                        </div>


                                        <div class="col-md-6">
                                            <label class="text-muted no-margin-bottom">Eligibility Code <span class="text-danger">*</span></label>
                                            <select id="eligibility" class="form-control select-Custom text-semibold" ng-model="formApp.eligibilityCode" ng-disabled="isReadOnly" style="width:100%;"
                                                    ng-options="c.eligibilityCode as c.eligibilityName for c in eligList" value="{{c.eligeligibilityCodeList}}">
                                                <option value="" selected="">-- Select --</option>
                                            </select>
                                        </div>

                                    </div>
                                </div>

                                <div class="form-group">
                                    <div class="row">

                                        <div class="col-md-3">
                                            <label class="text-muted no-margin-bottom">CSC Posted Date</label>
                                            <input id="text" ng-readonly="true" type="text" ng-model="(formatDate(formApp.CSCPostedDate))" class="form-control text-semibold">
                                        </div>
                                        <div class="col-md-3">
                                            <label class="text-muted no-margin-bottom"> CSC Closing Date</label>
                                            <input type="text" ng-readonly="true" ng-model="(formatDate(formApp.CSCClosingDate))" class="form-control text-semibold">
                                        </div>
                                        <div class="col-md-3">
                                            <label class="text-muted no-margin-bottom"> P.S.B. Date</label>
                                            <input type="text" ng-readonly="true" ng-model="(formatDate(formApp.PSBDate))" class="form-control text-semibold">
                                        </div>

                                        <div class="col-md-2">
                                            <label class="text-muted no-margin-bottom">Page #</label>
                                            <input type="text" ng-readonly="true" ng-model=" formApp.pageNo" class="form-control text-semibold">
                                        </div>

                                    </div>
